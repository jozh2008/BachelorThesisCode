<tool name="SAGA.grid_calculus.3" id="saga_grid_calculus_3" version="1.0.0">
  <description>Grid Difference</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayA False  isArrayB False  output_data_C $output_data_C  name SAGA.grid_calculus.3 A $A
B $B
prefer $prefer
response $response
outputType_C $OutputSection_C.outputType_C
transmissionMode_C $OutputSection_C.transmissionMode_C]]></command>
  <inputs>
    <param name="A" type="data" optional="false" label="A" help="A The following data types are allowed in the txt file:  x-ogc-aaigrid, x-ogc-envi, png, tiff" format="txt"/>
    <param name="B" type="data" optional="false" label="B" help="B The following data types are allowed in the txt file:  x-ogc-aaigrid, x-ogc-envi, png, tiff" format="txt"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_C" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_C" type="select" label="Difference (A - B)" help="Difference (A - B)">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_C" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_C" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_C" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_C" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_C" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Grid Difference]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
