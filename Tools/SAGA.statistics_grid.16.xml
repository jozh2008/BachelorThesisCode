<tool name="SAGA.statistics_grid.16" id="saga_statistics_grid_16" version="1.0.0">
  <description>Statistics for Grids from Files</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayFILES False  output_data_COUNT $output_data_COUNT  output_data_MIN $output_data_MIN  output_data_MAX $output_data_MAX  output_data_RANGE $output_data_RANGE  output_data_SUM $output_data_SUM  output_data_SUM2 $output_data_SUM2  output_data_MEAN $output_data_MEAN  output_data_VAR $output_data_VAR  output_data_STDDEV $output_data_STDDEV  output_data_HISTOGRAM $output_data_HISTOGRAM  output_data_QUANTILES $output_data_QUANTILES  name SAGA.statistics_grid.16 FILES $FILES
HCLASSES $HCLASSES
HRANGE $HRANGE
CUMULATIVE $CUMULATIVE
QUANTVALS '$QUANTVALS'
prefer $prefer
response $response
outputType_COUNT $OutputSection_COUNT.outputType_COUNT
transmissionMode_COUNT $OutputSection_COUNT.transmissionMode_COUNT
outputType_MIN $OutputSection_MIN.outputType_MIN
transmissionMode_MIN $OutputSection_MIN.transmissionMode_MIN
outputType_MAX $OutputSection_MAX.outputType_MAX
transmissionMode_MAX $OutputSection_MAX.transmissionMode_MAX
outputType_RANGE $OutputSection_RANGE.outputType_RANGE
transmissionMode_RANGE $OutputSection_RANGE.transmissionMode_RANGE
outputType_SUM $OutputSection_SUM.outputType_SUM
transmissionMode_SUM $OutputSection_SUM.transmissionMode_SUM
outputType_SUM2 $OutputSection_SUM2.outputType_SUM2
transmissionMode_SUM2 $OutputSection_SUM2.transmissionMode_SUM2
outputType_MEAN $OutputSection_MEAN.outputType_MEAN
transmissionMode_MEAN $OutputSection_MEAN.transmissionMode_MEAN
outputType_VAR $OutputSection_VAR.outputType_VAR
transmissionMode_VAR $OutputSection_VAR.transmissionMode_VAR
outputType_STDDEV $OutputSection_STDDEV.outputType_STDDEV
transmissionMode_STDDEV $OutputSection_STDDEV.transmissionMode_STDDEV
outputType_HISTOGRAM $OutputSection_HISTOGRAM.outputType_HISTOGRAM
transmissionMode_HISTOGRAM $OutputSection_HISTOGRAM.transmissionMode_HISTOGRAM
outputType_QUANTILES $OutputSection_QUANTILES.outputType_QUANTILES
transmissionMode_QUANTILES $OutputSection_QUANTILES.transmissionMode_QUANTILES]]></command>
  <inputs>
    <param name="FILES" type="data" optional="false" label="Grid Files" help="Grid Files The following data types are allowed in the txt file:  plain" format="txt"/>
    <param name="HCLASSES" type="integer" value="20" optional="true" label="Number of Classes" help="Number of Classes"/>
    <param name="HRANGE" type="select" optional="true" label="Range" help="Minimum and maximum values used to define the histogram classes.">
      <option selected="true" value="cell">cell</option>
      <option value="overall">overall</option>
    </param>
    <param name="CUMULATIVE" type="select" optional="true" label="Cumulative" help="Cumulative">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="QUANTVALS" type="text" value="5; 25; 50; 75; 95" optional="true" label="Percentiles" help="Separate the desired percentiles by semicolon"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_COUNT" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_COUNT" type="select" label="Number of Values" help="Number of Values">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_COUNT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_MIN" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
