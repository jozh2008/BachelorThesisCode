<tool name="SAGA.grid_tools.30" id="saga_grid_tools_30" version="1.0.0">
  <description>Transpose Grids</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayGRIDS True  output_data_TRANSPOSED $output_data_TRANSPOSED  name SAGA.grid_tools.30 GRIDS $GRIDS
MIRROR_X $MIRROR_X
MIRROR_Y $MIRROR_Y
prefer $prefer
response $response
outputType_TRANSPOSED $OutputSection_TRANSPOSED.outputType_TRANSPOSED
transmissionMode_TRANSPOSED $OutputSection_TRANSPOSED.transmissionMode_TRANSPOSED]]></command>
  <inputs>
    <param name="GRIDS" type="data" optional="false" label="Grids" help="Grids The following data types are allowed in the txt file:  png, x-ogc-envi, x-ogc-aaigrid, tiff" format="txt"/>
    <param name="MIRROR_X" type="select" optional="true" label="Mirror Horizontally" help="Mirror Horizontally">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="MIRROR_Y" type="select" optional="true" label="Mirror Vertically" help="Mirror Vertically">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_TRANSPOSED" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_TRANSPOSED" type="select" label="Transposed Grids" help="Transposed Grids">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_TRANSPOSED" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_TRANSPOSED" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TRANSPOSED" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_TRANSPOSED" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_TRANSPOSED" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Transpose Grids]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
