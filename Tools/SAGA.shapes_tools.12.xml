<tool name="SAGA.shapes_tools.12" id="saga_shapes_tools_12" version="1.0.0">
  <description>Create Graticule</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayEXTENT False  output_data_GRATICULE_LINE $output_data_GRATICULE_LINE  output_data_GRATICULE_RECT $output_data_GRATICULE_RECT  name SAGA.shapes_tools.12 TYPE $TYPE
EXTENT $EXTENT
EXTENT_X_MIN $EXTENT_X_MIN
EXTENT_X_MAX $EXTENT_X_MAX
EXTENT_Y_MIN $EXTENT_Y_MIN
EXTENT_Y_MAX $EXTENT_Y_MAX
DIVISION_X $DIVISION_X
DIVISION_Y $DIVISION_Y
ALIGNMENT $ALIGNMENT
prefer $prefer
response $response
outputType_GRATICULE_LINE $OutputSection_GRATICULE_LINE.outputType_GRATICULE_LINE
transmissionMode_GRATICULE_LINE $OutputSection_GRATICULE_LINE.transmissionMode_GRATICULE_LINE
outputType_GRATICULE_RECT $OutputSection_GRATICULE_RECT.outputType_GRATICULE_RECT
transmissionMode_GRATICULE_RECT $OutputSection_GRATICULE_RECT.transmissionMode_GRATICULE_RECT]]></command>
  <inputs>
    <param name="TYPE" type="select" optional="true" label="Type" help="Type">
      <option selected="true" value="Lines">Lines</option>
      <option value="Rectangles">Rectangles</option>
    </param>
    <param name="EXTENT" type="data" optional="false" label="Extent" help="Extent The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="EXTENT_X_MIN" type="float" optional="true" label="Width (min value)" help="Width"/>
    <param name="EXTENT_X_MAX" type="float" optional="true" label="Width (max value)" help="Width"/>
    <param name="EXTENT_Y_MIN" type="float" optional="true" label="Height (min value)" help="Height"/>
    <param name="EXTENT_Y_MAX" type="float" optional="true" label="Height (max value)" help="Height"/>
    <param name="DIVISION_X" type="float" value="10" optional="true" label="Division Width" help="Division Width"/>
    <param name="DIVISION_Y" type="float" value="10" optional="true" label="Division Height" help="Division Height"/>
    <param name="ALIGNMENT" type="select" optional="true" label="Alignment" help="Determines how the graticule is aligned to the extent, if division sizes do not fit.">
      <option selected="true" value="bottom-left">bottom-left</option>
      <option value="bottom-right">bottom-right</option>
      <option value="centered">centered</option>
      <option value="top-left">top-left</option>
      <option value="top-right">top-right</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_GRATICULE_LINE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_GRATICULE_LINE" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_GRATICULE_LINE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_GRATICULE_RECT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_GRATICULE_RECT" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_GRATICULE_RECT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_GRATICULE_LINE" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRATICULE_LINE" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_GRATICULE_LINE" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_GRATICULE_RECT" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRATICULE_RECT" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_GRATICULE_RECT" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[(c) 2004 by Victor Olaya. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
