<tool name="SAGA.shapes_points.12" id="saga_shapes_points_12" version="1.0.0">
  <description>Convex Hull</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArraySHAPES False  output_data_HULLS $output_data_HULLS  output_data_BOXES $output_data_BOXES  name SAGA.shapes_points.12 SHAPES $SHAPES
POLYPOINTS $POLYPOINTS
POLYGONCVX $POLYGONCVX
prefer $prefer
response $response
outputType_HULLS $OutputSection_HULLS.outputType_HULLS
transmissionMode_HULLS $OutputSection_HULLS.transmissionMode_HULLS
outputType_BOXES $OutputSection_BOXES.outputType_BOXES
transmissionMode_BOXES $OutputSection_BOXES.transmissionMode_BOXES]]></command>
  <inputs>
    <param name="SHAPES" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  vnd.google-earth.kml+xml, xml, json" format="txt"/>
    <param name="POLYPOINTS" type="select" optional="true" label="Hull Construction" help="This option does not apply to simple point layers.">
      <option value="one_hull_for_all_shapes">one hull for all shapes</option>
      <option selected="true" value="one_hull_per_shape">one hull per shape</option>
      <option value="one_hull_per_shape_part">one hull per shape part</option>
    </param>
    <param name="POLYGONCVX" type="select" optional="true" label="Polygon Convexity" help="Describes a polygon's compactness as ratio of its area to its hull's area.">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_HULLS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_HULLS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_HULLS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_BOXES" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_BOXES" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_BOXES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_HULLS" format="xml" label="output_data_HULLS" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_HULLS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_HULLS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_BOXES" format="xml" label="output_data_BOXES" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_BOXES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_BOXES" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <param name="response" value="document"/>
      <param name="SHAPES" value="http://zoo-project.org:8080/geoserver/wfs?service=WFS&amp;version=1.0.0&amp;request=GetFeature&amp;srsName=EPSG:4326&amp;typename=sf:bugsites"/>
      <output name="output_data_HULLS" ftype="txt" value="output_data_HULLS.txt"/>
      <output name="output_data_BOXES" ftype="txt" value="output_data_BOXES.txt"/>
    </test>
    <test>
      <param name="response" value="document"/>
      <param name="SHAPES" value="http://zoo-project.org:8080/geoserver/wfs?service=WFS&amp;version=1.0.0&amp;request=GetFeature&amp;srsName=EPSG:4326&amp;typename=sf:bugsites"/>
      <output name="output_data_HULLS" ftype="txt" value="output_data_HULLS.txt"/>
      <output name="output_data_BOXES" ftype="txt" value="output_data_BOXES.txt"/>
    </test>
  </tests>
  <help><![CDATA[Implementation of 'Andrew's Monotone Chain Algorithm' for convex hull construction. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
