<tool name="SAGA.db_pgsql.0" id="saga_db_pgsql_0" version="1.0.0">
  <description>List PostgreSQL Connections</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py output_data_CONNECTIONS $output_data_CONNECTIONS  name SAGA.db_pgsql.0 prefer $prefer
response $response
outputType_CONNECTIONS $OutputSection_CONNECTIONS.outputType_CONNECTIONS
transmissionMode_CONNECTIONS $OutputSection_CONNECTIONS.transmissionMode_CONNECTIONS]]></command>
  <inputs>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_CONNECTIONS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CONNECTIONS" type="select" label="Connections" help="Connections">
        <option value="text/csv">csv</option>
      </param>
      <param name="transmissionMode_CONNECTIONS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_CONNECTIONS" format="csv" hidden="false"/>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Lists all PostgreSQL sources.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
