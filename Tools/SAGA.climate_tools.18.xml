<tool name="SAGA.climate_tools.18" id="saga_climate_tools_18" version="1.0.0">
  <description>Growing Degree Days</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayTMEAN True  output_data_NGDD $output_data_NGDD  output_data_TSUM $output_data_TSUM  output_data_FIRST $output_data_FIRST  output_data_LAST $output_data_LAST  name SAGA.climate_tools.18 TMEAN $TMEAN
TBASE $TBASE
prefer $prefer
response $response
outputType_NGDD $OutputSection_NGDD.outputType_NGDD
transmissionMode_NGDD $OutputSection_NGDD.transmissionMode_NGDD
outputType_TSUM $OutputSection_TSUM.outputType_TSUM
transmissionMode_TSUM $OutputSection_TSUM.transmissionMode_TSUM
outputType_FIRST $OutputSection_FIRST.outputType_FIRST
transmissionMode_FIRST $OutputSection_FIRST.transmissionMode_FIRST
outputType_LAST $OutputSection_LAST.outputType_LAST
transmissionMode_LAST $OutputSection_LAST.transmissionMode_LAST]]></command>
  <inputs>
    <param name="TMEAN" type="data" optional="false" label="Mean Monthly Temperatures" help="Monthly (12) temperature observations. The following data types are allowed in the txt file:  tiff, png, x-ogc-envi, x-ogc-aaigrid" format="txt"/>
    <param name="TBASE" type="float" value="0" optional="true" label="Base Temperature" help="Base temperature in degree Celsius"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_NGDD" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_NGDD" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_NGDD" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_TSUM" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_TSUM" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_TSUM" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_FIRST" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_FIRST" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_FIRST" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_LAST" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_LAST" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_LAST" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_NGDD" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_NGDD" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_NGDD" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_NGDD" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_TSUM" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TSUM" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_TSUM" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_TSUM" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_FIRST" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_FIRST" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_FIRST" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_FIRST" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_LAST" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_LAST" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_LAST" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_LAST" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[This tool calculates growing degree days from daily or from spline interpolated monthly observations.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
