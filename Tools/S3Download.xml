<tool name="S3Download" id="s3download" version="2.0.0">
  <description>Echo input</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py output_data_Result $output_data_Result  name S3Download endpoint_url '$endpoint_url'
access_key_id '$access_key_id'
secret_access_key '$secret_access_key'
region '$region'
s3_bucket '$s3_bucket'
s3_path '$s3_path'
band $band
resolution $resolution
prefer $prefer
response $response
outputType_Result $OutputSection_Result.outputType_Result
transmissionMode_Result $OutputSection_Result.transmissionMode_Result]]></command>
  <inputs>
    <param name="endpoint_url" type="text" optional="false" label="Literal Input (string)" help="An input string"/>
    <param name="access_key_id" type="text" optional="false" label="Literal Input (string)" help="An input string"/>
    <param name="secret_access_key" type="text" optional="false" label="Literal Input (string)" help="An input string"/>
    <param name="region" type="text" optional="false" label="Literal Input (string)" help="An input string"/>
    <param name="s3_bucket" type="text" optional="false" label="Literal Input (string)" help="An input string"/>
    <param name="s3_path" type="text" optional="false" label="Literal Input (string)" help="An input string"/>
    <param name="band" type="select" optional="false" label="Literal Input (string)" help="An input string">
      <option value="AOT">AOT</option>
      <option value="B01">B01</option>
      <option value="B02">B02</option>
      <option value="B03">B03</option>
      <option value="B04">B04</option>
      <option value="B05">B05</option>
      <option value="B06">B06</option>
      <option value="B07">B07</option>
      <option value="B08">B08</option>
      <option value="B09">B09</option>
      <option value="B11">B11</option>
      <option value="B12">B12</option>
      <option value="B8A">B8A</option>
      <option value="SCL">SCL</option>
      <option value="TCL">TCL</option>
      <option value="WVP">WVP</option>
    </param>
    <param name="resolution" type="select" optional="false" label="Literal Input (string)" help="An input string">
      <option value="10m">10m</option>
      <option value="20m">20m</option>
      <option value="60m">60m</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_Result" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_Result" type="select" label="Author did not provide help for this parameter... ">
        <option value="image/jp2">jp2</option>
        <option value="ogc:geotiff">ogc:geotiff</option>
      </param>
      <param name="transmissionMode_Result" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_Result" format="jp2" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_Result" format="ogc:geotiff" value="ogc:geotiff"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Simply echo the value provided as input]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
