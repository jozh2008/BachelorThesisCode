<tool name="SAGA.grid_analysis.8" id="saga_grid_analysis_8" version="1.0.0">
  <description>Pattern Analysis</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayINPUT False  output_data_RELATIVE $output_data_RELATIVE  output_data_DIVERSITY $output_data_DIVERSITY  output_data_DOMINANCE $output_data_DOMINANCE  output_data_FRAGMENTATION $output_data_FRAGMENTATION  output_data_NDC $output_data_NDC  output_data_CVN $output_data_CVN  name SAGA.grid_analysis.8 INPUT $INPUT
WINSIZE $WINSIZE
MAXNUMCLASS $MAXNUMCLASS
prefer $prefer
response $response
outputType_RELATIVE $OutputSection_RELATIVE.outputType_RELATIVE
transmissionMode_RELATIVE $OutputSection_RELATIVE.transmissionMode_RELATIVE
outputType_DIVERSITY $OutputSection_DIVERSITY.outputType_DIVERSITY
transmissionMode_DIVERSITY $OutputSection_DIVERSITY.transmissionMode_DIVERSITY
outputType_DOMINANCE $OutputSection_DOMINANCE.outputType_DOMINANCE
transmissionMode_DOMINANCE $OutputSection_DOMINANCE.transmissionMode_DOMINANCE
outputType_FRAGMENTATION $OutputSection_FRAGMENTATION.outputType_FRAGMENTATION
transmissionMode_FRAGMENTATION $OutputSection_FRAGMENTATION.transmissionMode_FRAGMENTATION
outputType_NDC $OutputSection_NDC.outputType_NDC
transmissionMode_NDC $OutputSection_NDC.transmissionMode_NDC
outputType_CVN $OutputSection_CVN.outputType_CVN
transmissionMode_CVN $OutputSection_CVN.transmissionMode_CVN]]></command>
  <inputs>
    <param name="INPUT" type="data" optional="false" label="Input Grid" help="Input Grid The following data types are allowed in the txt file:  png, x-ogc-envi, x-ogc-aaigrid, tiff" format="txt"/>
    <param name="WINSIZE" type="select" optional="true" label="Size of Analysis Window" help="Size of Analysis Window">
      <option selected="true" value="3_X_3">3 X 3</option>
      <option value="5_X_5">5 X 5</option>
      <option value="7_X_7">7 X 7</option>
    </param>
    <param name="MAXNUMCLASS" type="integer" value="10" optional="true" label="Max. Number of Classes" help="Maximum number of classes in entire grid."/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_RELATIVE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_RELATIVE" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_RELATIVE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_DIVERSITY" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_DIVERSITY" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_DIVERSITY" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_DOMINANCE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_DOMINANCE" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_DOMINANCE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_FRAGMENTATION" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_FRAGMENTATION" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_FRAGMENTATION" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_NDC" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_NDC" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_NDC" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_CVN" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CVN" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_CVN" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_RELATIVE" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_RELATIVE" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_RELATIVE" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_RELATIVE" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_DIVERSITY" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_DIVERSITY" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_DIVERSITY" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_DIVERSITY" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_DOMINANCE" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_DOMINANCE" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_DOMINANCE" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_DOMINANCE" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_FRAGMENTATION" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_FRAGMENTATION" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_FRAGMENTATION" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_FRAGMENTATION" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_NDC" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_NDC" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_NDC" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_NDC" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_CVN" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_CVN" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_CVN" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_CVN" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[(c) 2004 by Victor Olaya. Pattern Analysis]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
