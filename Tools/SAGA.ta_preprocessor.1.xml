<tool name="SAGA.ta_preprocessor.1" id="saga_ta_preprocessor_1" version="1.0.0">
  <description>Sink Drainage Route Detection</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayELEVATION False  output_data_SINKROUTE $output_data_SINKROUTE  name SAGA.ta_preprocessor.1 ELEVATION $ELEVATION
THRESHOLD $THRESHOLD
THRSHEIGHT $THRSHEIGHT
prefer $prefer
response $response
outputType_SINKROUTE $OutputSection_SINKROUTE.outputType_SINKROUTE
transmissionMode_SINKROUTE $OutputSection_SINKROUTE.transmissionMode_SINKROUTE]]></command>
  <inputs>
    <param name="ELEVATION" type="data" optional="false" label="Elevation" help="Elevation The following data types are allowed in the txt file:  x-ogc-aaigrid, png, tiff, x-ogc-envi" format="txt"/>
    <param name="THRESHOLD" type="select" optional="true" label="Threshold" help="Threshold">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="THRSHEIGHT" type="float" value="100" optional="true" label="Threshold Height" help="Threshold Height"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_SINKROUTE" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_SINKROUTE" type="select" label="Sink Route" help="Sink Route">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_SINKROUTE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_SINKROUTE" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SINKROUTE" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_SINKROUTE" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_SINKROUTE" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Sink Drainage Route Detection]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
