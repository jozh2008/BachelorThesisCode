<tool name="SAGA.grids_tools.4" id="saga_grids_tools_4" version="1.0.0">
  <description>Add a Grid to a Grid Collection</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayGRID False  output_data_GRIDS $output_data_GRIDS  name SAGA.grids_tools.4 GRID $GRID
Z_LEVEL $Z_LEVEL
DELETE $DELETE
prefer $prefer
response $response
outputType_GRIDS $OutputSection_GRIDS.outputType_GRIDS
transmissionMode_GRIDS $OutputSection_GRIDS.transmissionMode_GRIDS]]></command>
  <inputs>
    <param name="GRID" type="data" optional="false" label="Grid" help="Grid The following data types are allowed in the txt file:  x-ogc-envi, x-ogc-aaigrid, png, tiff" format="txt"/>
    <param name="Z_LEVEL" type="float" value="0" optional="true" label="Z" help="Z"/>
    <param name="DELETE" type="select" optional="true" label="Delete" help="Delete">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_GRIDS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_GRIDS" type="select" label="Grid Collection" help="Grid Collection">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_GRIDS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_GRIDS" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRIDS" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_GRIDS" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_GRIDS" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Adds a grid at the specified z-level to an existing grid collection. If no grid collection is supplied it will be created according to the input grid's grid system and data type. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
