<tool name="SAGA.shapes_tools.15" id="saga_shapes_tools_15" version="1.0.0">
  <description>Split Shapes Layer</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArraySHAPES False  output_data_CUTS $output_data_CUTS  output_data_EXTENT $output_data_EXTENT  name SAGA.shapes_tools.15 SHAPES $SHAPES
NX $NX
NY $NY
METHOD $METHOD
prefer $prefer
response $response
outputType_CUTS $OutputSection_CUTS.outputType_CUTS
transmissionMode_CUTS $OutputSection_CUTS.transmissionMode_CUTS
outputType_EXTENT $OutputSection_EXTENT.outputType_EXTENT
transmissionMode_EXTENT $OutputSection_EXTENT.transmissionMode_EXTENT]]></command>
  <inputs>
    <param name="SHAPES" type="data" optional="false" label="Shapes" help="Shapes The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="NX" type="integer" value="2" optional="true" label="Number of horizontal tiles" help="Number of horizontal tiles"/>
    <param name="NY" type="integer" value="2" optional="true" label="Number of vertical tiles" help="Number of vertical tiles"/>
    <param name="METHOD" type="select" optional="true" label="Method" help="Method">
      <option value="center">center</option>
      <option selected="true" value="completely_contained">completely contained</option>
      <option value="intersects">intersects</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_CUTS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CUTS" type="select" label="Tiles" help="Tiles">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_CUTS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_EXTENT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_EXTENT" type="select" label="Extent" help="Extent">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_EXTENT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_CUTS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_CUTS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_CUTS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_EXTENT" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_EXTENT" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_EXTENT" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Split Shapes Layer]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
