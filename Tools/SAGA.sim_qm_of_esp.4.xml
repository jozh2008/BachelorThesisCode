<tool name="SAGA.sim_qm_of_esp.4" id="saga_sim_qm_of_esp_4" version="1.0.0">
  <description>Diffusive Hillslope Evolution (ADI)</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayDEM False  isArrayCHANNELS False  output_data_MODEL $output_data_MODEL  output_data_DIFF $output_data_DIFF  name SAGA.sim_qm_of_esp.4 DEM $DEM
CHANNELS $CHANNELS
UPDATE $UPDATE
KAPPA $KAPPA
DURATION $DURATION
TIMESTEP $TIMESTEP
DTIME $DTIME
prefer $prefer
response $response
outputType_MODEL $OutputSection_MODEL.outputType_MODEL
transmissionMode_MODEL $OutputSection_MODEL.transmissionMode_MODEL
outputType_DIFF $OutputSection_DIFF.outputType_DIFF
transmissionMode_DIFF $OutputSection_DIFF.transmissionMode_DIFF]]></command>
  <inputs>
    <param name="DEM" type="data" optional="false" label="Elevation" help="Elevation The following data types are allowed in the txt file:  png, x-ogc-envi, x-ogc-aaigrid, tiff" format="txt"/>
    <param name="CHANNELS" type="data" optional="false" label="Channel Mask" help="use a zero value for hillslopes, any other value for channel cells. The following data types are allowed in the txt file:  png, x-ogc-envi, x-ogc-aaigrid, tiff" format="txt"/>
    <param name="UPDATE" type="select" optional="true" label="Update" help="Update">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="KAPPA" type="float" value="10" optional="true" label="Diffusivity [m2 / kyr]" help="Diffusivity [m2 / kyr]"/>
    <param name="DURATION" type="float" value="10000" optional="true" label="Simulation Time [kyr]" help="Simulation Time [kyr]"/>
    <param name="TIMESTEP" type="select" optional="true" label="Time Step" help="Time Step">
      <option value="automatically">automatically</option>
      <option selected="true" value="user_defined">user defined</option>
    </param>
    <param name="DTIME" type="float" value="1000" optional="true" label="Time Step [kyr]" help="Time Step [kyr]"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_MODEL" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_MODEL" type="select" label="Modelled Elevation" help="Modelled Elevation">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_MODEL" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_DIFF" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_DIFF" type="select" label="Elevation Difference" help="Elevation Difference">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_DIFF" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_MODEL" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_MODEL" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_MODEL" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_MODEL" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_DIFF" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_DIFF" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_DIFF" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_DIFF" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Simulation of diffusive hillslope evolution using an Alternating-Direction-Implicit (ADI) method.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
