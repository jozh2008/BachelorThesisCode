<tool name="SAGA.climate_tools.9" id="saga_climate_tools_9" version="1.0.0">
  <description>Sunrise and Sunset</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayTARGET False  output_data_SUNRISE $output_data_SUNRISE  output_data_SUNSET $output_data_SUNSET  output_data_LENGTH $output_data_LENGTH  name SAGA.climate_tools.9 TARGET $TARGET
DAY_MIN '$DAY_MIN'
TIME $TIME
prefer $prefer
response $response
outputType_SUNRISE $OutputSection_SUNRISE.outputType_SUNRISE
transmissionMode_SUNRISE $OutputSection_SUNRISE.transmissionMode_SUNRISE
outputType_SUNSET $OutputSection_SUNSET.outputType_SUNSET
transmissionMode_SUNSET $OutputSection_SUNSET.transmissionMode_SUNSET
outputType_LENGTH $OutputSection_LENGTH.outputType_LENGTH
transmissionMode_LENGTH $OutputSection_LENGTH.transmissionMode_LENGTH]]></command>
  <inputs>
    <param name="TARGET" type="data" optional="false" label="Target System" help="Target System The following data types are allowed in the txt file:  x-ogc-aaigrid, x-ogc-envi, tiff, png" format="txt"/>
    <param name="DAY_MIN" type="text" optional="true" label="Day of Month (min value)" help="Day of Month"/>
    <param name="TIME" type="select" optional="true" label="Time" help="Time">
      <option selected="true" value="local">local</option>
      <option value="world">world</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_SUNRISE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_SUNRISE" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_SUNRISE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_SUNSET" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_SUNSET" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_SUNSET" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_LENGTH" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_LENGTH" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_LENGTH" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_SUNRISE" format="tiff" label="output_data_SUNRISE" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SUNRISE" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_SUNRISE" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_SUNRISE" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_SUNSET" format="tiff" label="output_data_SUNSET" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SUNSET" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_SUNSET" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_SUNSET" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_LENGTH" format="tiff" label="output_data_LENGTH" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_LENGTH" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_LENGTH" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_LENGTH" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <param name="response" value="document"/>
      <output name="output_data_SUNRISE" ftype="txt" value="output_data_SUNRISE.txt"/>
      <output name="output_data_SUNSET" ftype="txt" value="output_data_SUNSET.txt"/>
      <output name="output_data_LENGTH" ftype="txt" value="output_data_LENGTH.txt"/>
    </test>
  </tests>
  <help><![CDATA[This tool calculates the time of sunrise and sunset and the resulting day length for each cell of the target grid. The target grid needs to provide information about its coordinate system. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
