<tool name="SAGA.ta_hydrology.18" id="saga_ta_hydrology_18" version="1.0.0">
  <description>Flow Accumulation (Mass-Flux Method)</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayDEM False  output_data_AREA $output_data_AREA  output_data_G_SLOPE $output_data_G_SLOPE  output_data_G_ASPECT $output_data_G_ASPECT  output_data_G_AREA $output_data_G_AREA  output_data_G_FLOW $output_data_G_FLOW  name SAGA.ta_hydrology.18 DEM $DEM
METHOD $METHOD
B_SLOPE $B_SLOPE
B_ASPECT $B_ASPECT
B_AREA $B_AREA
B_FLOW $B_FLOW
prefer $prefer
response $response
outputType_AREA $OutputSection_AREA.outputType_AREA
transmissionMode_AREA $OutputSection_AREA.transmissionMode_AREA
outputType_G_SLOPE $OutputSection_G_SLOPE.outputType_G_SLOPE
transmissionMode_G_SLOPE $OutputSection_G_SLOPE.transmissionMode_G_SLOPE
outputType_G_ASPECT $OutputSection_G_ASPECT.outputType_G_ASPECT
transmissionMode_G_ASPECT $OutputSection_G_ASPECT.transmissionMode_G_ASPECT
outputType_G_AREA $OutputSection_G_AREA.outputType_G_AREA
transmissionMode_G_AREA $OutputSection_G_AREA.transmissionMode_G_AREA
outputType_G_FLOW $OutputSection_G_FLOW.outputType_G_FLOW
transmissionMode_G_FLOW $OutputSection_G_FLOW.transmissionMode_G_FLOW]]></command>
  <inputs>
    <param name="DEM" type="data" optional="false" label="Elevation" help="Elevation The following data types are allowed in the txt file:  png, x-ogc-aaigrid, x-ogc-envi, tiff" format="txt"/>
    <param name="METHOD" type="select" optional="true" label="Flow Split Method" help="Flow Split Method">
      <option value="cell_area">cell area</option>
      <option selected="true" value="flow_width_(original)">flow width (original)</option>
    </param>
    <param name="B_SLOPE" type="select" optional="true" label="Slope" help="Slope">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="B_ASPECT" type="select" optional="true" label="Aspect" help="Aspect">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="B_AREA" type="select" optional="true" label="Flow Accumulation" help="Flow Accumulation">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="B_FLOW" type="select" optional="true" label="Flow Lines" help="Flow Lines">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_AREA" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_AREA" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_AREA" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_G_SLOPE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_G_SLOPE" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_G_SLOPE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_G_ASPECT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_G_ASPECT" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_G_ASPECT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_G_AREA" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_G_AREA" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_G_AREA" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_G_FLOW" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_G_FLOW" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_G_FLOW" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_AREA" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_AREA" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_AREA" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_AREA" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_G_SLOPE" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_G_SLOPE" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_G_SLOPE" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_G_SLOPE" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_G_ASPECT" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_G_ASPECT" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_G_ASPECT" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_G_ASPECT" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_G_AREA" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_G_AREA" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_G_AREA" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_G_AREA" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_G_FLOW" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_G_FLOW" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_G_FLOW" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_G_FLOW" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[The Mass-Flux Method (MFM) for the DEM based calculation of flow accumulation as proposed by Gruber and Peckham (2008).&lt;br/&gt;&lt;br/&gt;!!!UNDER DEVELOPMENT!!! To be done: solving the streamline resolution problem]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
