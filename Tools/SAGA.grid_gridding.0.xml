<tool name="SAGA.grid_gridding.0" id="saga_grid_gridding_0" version="1.0.0">
  <description>Shapes to Grid</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayINPUT False  isArrayFIELD False  isArrayTARGET_TEMPLATE False  output_data_GRID $output_data_GRID  output_data_COUNT $output_data_COUNT  name SAGA.grid_gridding.0 INPUT $INPUT
FIELD $FIELD
OUTPUT $OUTPUT
MULTIPLE $MULTIPLE
LINE_TYPE $LINE_TYPE
POLY_TYPE $POLY_TYPE
GRID_TYPE $GRID_TYPE
TARGET_DEFINITION $TARGET_DEFINITION
TARGET_USER_SIZE $TARGET_USER_SIZE
TARGET_USER_XMIN $TARGET_USER_XMIN
TARGET_USER_XMAX $TARGET_USER_XMAX
TARGET_USER_YMIN $TARGET_USER_YMIN
TARGET_USER_YMAX $TARGET_USER_YMAX
TARGET_USER_COLS $TARGET_USER_COLS
TARGET_USER_ROWS $TARGET_USER_ROWS
TARGET_USER_FITS $TARGET_USER_FITS
TARGET_TEMPLATE $TARGET_TEMPLATE
prefer $prefer
response $response
outputType_GRID $OutputSection_GRID.outputType_GRID
transmissionMode_GRID $OutputSection_GRID.transmissionMode_GRID
outputType_COUNT $OutputSection_COUNT.outputType_COUNT
transmissionMode_COUNT $OutputSection_COUNT.transmissionMode_COUNT]]></command>
  <inputs>
    <param name="INPUT" type="data" optional="false" label="Shapes" help="Shapes The following data types are allowed in the txt file:  xml, vnd.google-earth.kml+xml, json" format="txt"/>
    <param name="FIELD" type="data" optional="false" label="Attribute" help="Attribute The following data types are allowed in the txt file:  csv" format="txt"/>
    <param name="OUTPUT" type="select" optional="true" label="Output Values" help="Output Values">
      <option selected="true" value="attribute">attribute</option>
      <option value="data_/_no-data">data / no-data</option>
      <option value="index_number">index number</option>
    </param>
    <param name="MULTIPLE" type="select" optional="true" label="Method for Multiple Values" help="Method for Multiple Values">
      <option value="first">first</option>
      <option selected="true" value="last">last</option>
      <option value="maximum">maximum</option>
      <option value="mean">mean</option>
      <option value="minimum">minimum</option>
    </param>
    <param name="LINE_TYPE" type="select" optional="true" label="Lines" help="Lines">
      <option selected="true" value="thick">thick</option>
      <option value="thin">thin</option>
    </param>
    <param name="POLY_TYPE" type="select" optional="true" label="Polygon" help="Polygon">
      <option selected="true" value="cell">cell</option>
      <option value="node">node</option>
    </param>
    <param name="GRID_TYPE" type="select" optional="true" label="Data Type" help="Data Type">
      <option value="1_bit">1 bit</option>
      <option value="1_byte_signed_integer">1 byte signed integer</option>
      <option value="1_byte_unsigned_integer">1 byte unsigned integer</option>
      <option value="2_byte_signed_integer">2 byte signed integer</option>
      <option value="2_byte_unsigned_integer">2 byte unsigned integer</option>
      <option value="4_byte_floating_point">4 byte floating point</option>
      <option value="4_byte_signed_integer">4 byte signed integer</option>
      <option value="4_byte_unsigned_integer">4 byte unsigned integer</option>
      <option value="8_byte_floating_point">8 byte floating point</option>
      <option selected="true" value="same_as_attribute">same as attribute</option>
    </param>
    <param name="TARGET_DEFINITION" type="select" optional="true" label="Target Grid System" help="Target Grid System">
      <option value="grid_or_grid_system">grid or grid system</option>
      <option selected="true" value="user_defined">user defined</option>
    </param>
    <param name="TARGET_USER_SIZE" type="float" value="1" optional="true" label="Cellsize" help="Cellsize"/>
    <param name="TARGET_USER_XMIN" type="float" value="0" optional="true" label="West" help="West"/>
    <param name="TARGET_USER_XMAX" type="float" value="100" optional="true" label="East" help="East"/>
    <param name="TARGET_USER_YMIN" type="float" value="0" optional="true" label="South" help="South"/>
    <param name="TARGET_USER_YMAX" type="float" value="100" optional="true" label="North" help="North"/>
    <param name="TARGET_USER_COLS" type="integer" value="100" optional="true" label="Columns" help="Number of cells in East-West direction."/>
    <param name="TARGET_USER_ROWS" type="integer" value="100" optional="true" label="Rows" help="Number of cells in North-South direction."/>
    <param name="TARGET_USER_FITS" type="select" optional="true" label="Fit" help="Fit">
      <option value="cells">cells</option>
      <option selected="true" value="nodes">nodes</option>
    </param>
    <param name="TARGET_TEMPLATE" type="data" optional="false" label="Target System" help="use this grid's system for output grids The following data types are allowed in the txt file:  tiff, x-ogc-aaigrid, x-ogc-envi, png" format="txt"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_GRID" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_GRID" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_GRID" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_COUNT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_COUNT" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_COUNT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_GRID" format="tiff" label="output_data_GRID" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRID" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_GRID" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_GRID" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_COUNT" format="tiff" label="output_data_COUNT" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_COUNT" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_COUNT" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_COUNT" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <param name="response" value="document"/>
      <output name="output_data_GRID" ftype="txt" value="output_data_GRID.txt"/>
      <output name="output_data_COUNT" ftype="txt" value="output_data_COUNT.txt"/>
    </test>
  </tests>
  <help><![CDATA[Gridding of a shapes layer. If some shapes are selected, only these will be gridded.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
