<tool name="SAGA.shapes_grid.5" id="saga_shapes_grid_5" version="1.0.0">
  <description>Contour Lines from Grid</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayGRID False  output_data_CONTOUR $output_data_CONTOUR  output_data_POLYGONS $output_data_POLYGONS  name SAGA.shapes_grid.5 GRID $GRID
VERTEX $VERTEX
SCALE $SCALE
LINE_PARTS $LINE_PARTS
POLY_PARTS $POLY_PARTS
ZMIN $ZMIN
ZMAX $ZMAX
ZSTEP $ZSTEP
prefer $prefer
response $response
outputType_CONTOUR $OutputSection_CONTOUR.outputType_CONTOUR
transmissionMode_CONTOUR $OutputSection_CONTOUR.transmissionMode_CONTOUR
outputType_POLYGONS $OutputSection_POLYGONS.outputType_POLYGONS
transmissionMode_POLYGONS $OutputSection_POLYGONS.transmissionMode_POLYGONS]]></command>
  <inputs>
    <param name="GRID" type="data" optional="false" label="Grid" help="Grid The following data types are allowed in the txt file:  png, tiff, x-ogc-envi, x-ogc-aaigrid" format="txt"/>
    <param name="VERTEX" type="select" optional="true" label="Vertex Type" help="choose vertex type for resulting contours">
      <option selected="true" value="x,_y">x, y</option>
    </param>
    <param name="SCALE" type="float" value="1" optional="true" label="Interpolation Scale" help="set greater one for line smoothing"/>
    <param name="LINE_PARTS" type="select" optional="true" label="Split Parts" help="Split Parts">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="POLY_PARTS" type="select" optional="true" label="Split Polygon Parts" help="Split Polygon Parts">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="ZMIN" type="float" value="0" optional="true" label="Minimum Contour Value" help="Minimum Contour Value"/>
    <param name="ZMAX" type="float" value="10000" optional="true" label="Maximum Contour Value" help="Maximum Contour Value"/>
    <param name="ZSTEP" type="float" value="10" optional="true" label="Equidistance" help="Equidistance"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_CONTOUR" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CONTOUR" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_CONTOUR" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_POLYGONS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_POLYGONS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_POLYGONS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_CONTOUR" format="xml" label="output_data_CONTOUR" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_CONTOUR" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_CONTOUR" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_POLYGONS" format="xml" label="output_data_POLYGONS" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_POLYGONS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_POLYGONS" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <param name="response" value="document"/>
      <output name="output_data_CONTOUR" ftype="txt" value="output_data_CONTOUR.txt"/>
      <output name="output_data_POLYGONS" ftype="txt" value="output_data_POLYGONS.txt"/>
    </test>
  </tests>
  <help><![CDATA[Derive contour lines (isolines) from grid. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
