<tool name="SAGA.tin_tools.1" id="saga_tin_tools_1" version="1.0.0">
  <description>Grid to TIN (Surface Specific Points)</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayGRID False  isArrayVALUES True  output_data_TIN_POINTS $output_data_TIN_POINTS  output_data_TIN_CENTER $output_data_TIN_CENTER  output_data_TIN_EDGES $output_data_TIN_EDGES  output_data_TIN_TRIANGLES $output_data_TIN_TRIANGLES  output_data_TIN_POLYGONS $output_data_TIN_POLYGONS  name SAGA.tin_tools.1 GRID $GRID
VALUES $VALUES
METHOD $METHOD
HIGH $HIGH
FLOW_MIN $FLOW_MIN
FLOW_MAX $FLOW_MAX
PEUCKER $PEUCKER
prefer $prefer
response $response
outputType_TIN_POINTS $OutputSection_TIN_POINTS.outputType_TIN_POINTS
transmissionMode_TIN_POINTS $OutputSection_TIN_POINTS.transmissionMode_TIN_POINTS
outputType_TIN_CENTER $OutputSection_TIN_CENTER.outputType_TIN_CENTER
transmissionMode_TIN_CENTER $OutputSection_TIN_CENTER.transmissionMode_TIN_CENTER
outputType_TIN_EDGES $OutputSection_TIN_EDGES.outputType_TIN_EDGES
transmissionMode_TIN_EDGES $OutputSection_TIN_EDGES.transmissionMode_TIN_EDGES
outputType_TIN_TRIANGLES $OutputSection_TIN_TRIANGLES.outputType_TIN_TRIANGLES
transmissionMode_TIN_TRIANGLES $OutputSection_TIN_TRIANGLES.transmissionMode_TIN_TRIANGLES
outputType_TIN_POLYGONS $OutputSection_TIN_POLYGONS.outputType_TIN_POLYGONS
transmissionMode_TIN_POLYGONS $OutputSection_TIN_POLYGONS.transmissionMode_TIN_POLYGONS]]></command>
  <inputs>
    <param name="GRID" type="data" optional="false" label="Grid" help="Grid The following data types are allowed in the txt file:  png, x-ogc-aaigrid, tiff, x-ogc-envi" format="txt"/>
    <param name="VALUES" type="data" optional="false" label="Values" help="Values The following data types are allowed in the txt file:  png, x-ogc-aaigrid, tiff, x-ogc-envi" format="txt"/>
    <param name="METHOD" type="select" optional="true" label="Method" help="The method used to identify surface specific points.">
      <option value="Flow_Direction">Flow Direction</option>
      <option value="Flow_Direction_(up_and_down)">Flow Direction (up and down)</option>
      <option value="Mark_Highest_Neighbour">Mark Highest Neighbour</option>
      <option selected="true" value="Opposite_Neighbours">Opposite Neighbours</option>
      <option value="Peucker_&amp;_Douglas">Peucker &amp; Douglas</option>
    </param>
    <param name="HIGH" type="integer" value="4" optional="true" label="Mark Highest Neighbour" help="Mark Highest Neighbour"/>
    <param name="FLOW_MIN" type="float" optional="true" label="Flow Direction (min value)" help="Flow Direction"/>
    <param name="FLOW_MAX" type="float" optional="true" label="Flow Direction (max value)" help="Flow Direction"/>
    <param name="PEUCKER" type="float" value="2" optional="true" label="Peucker &amp; Douglas" help="Peucker &amp; Douglas"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_TIN_POINTS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_TIN_POINTS" type="select" label="TINPOINTS Shapes" help="TIN">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_TIN_POINTS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_TIN_CENTER" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_TIN_CENTER" type="select" label="TINCENTER Shapes" help="TIN">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_TIN_CENTER" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_TIN_EDGES" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_TIN_EDGES" type="select" label="TINEDGES Shapes" help="TIN">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_TIN_EDGES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_TIN_TRIANGLES" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_TIN_TRIANGLES" type="select" label="TINTRIANGLES Shapes" help="TIN">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_TIN_TRIANGLES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_TIN_POLYGONS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_TIN_POLYGONS" type="select" label="TINPOLYGONS Shapes" help="TIN">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_TIN_POLYGONS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_TIN_POINTS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TIN_POINTS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_TIN_POINTS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_TIN_CENTER" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TIN_CENTER" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_TIN_CENTER" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_TIN_EDGES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TIN_EDGES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_TIN_EDGES" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_TIN_TRIANGLES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TIN_TRIANGLES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_TIN_TRIANGLES" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_TIN_POLYGONS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_TIN_POLYGONS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_TIN_POLYGONS" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Creates a TIN by identifying (surface) specific points of a grid.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
