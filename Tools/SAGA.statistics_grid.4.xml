WEIGHTS $WEIGHTS
RESAMPLING $RESAMPLING
PCTL_VAL $PCTL_VAL
prefer $prefer
response $response
outputType_MEAN $OutputSection_MEAN.outputType_MEAN
transmissionMode_MEAN $OutputSection_MEAN.transmissionMode_MEAN
outputType_MIN $OutputSection_MIN.outputType_MIN
transmissionMode_MIN $OutputSection_MIN.transmissionMode_MIN
outputType_MAX $OutputSection_MAX.outputType_MAX
transmissionMode_MAX $OutputSection_MAX.transmissionMode_MAX
outputType_RANGE $OutputSection_RANGE.outputType_RANGE
transmissionMode_RANGE $OutputSection_RANGE.transmissionMode_RANGE
outputType_SUM $OutputSection_SUM.outputType_SUM
transmissionMode_SUM $OutputSection_SUM.transmissionMode_SUM
outputType_SUM2 $OutputSection_SUM2.outputType_SUM2
transmissionMode_SUM2 $OutputSection_SUM2.transmissionMode_SUM2
outputType_VAR $OutputSection_VAR.outputType_VAR
transmissionMode_VAR $OutputSection_VAR.transmissionMode_VAR
outputType_STDDEV $OutputSection_STDDEV.outputType_STDDEV
transmissionMode_STDDEV $OutputSection_STDDEV.transmissionMode_STDDEV
outputType_STDDEVLO $OutputSection_STDDEVLO.outputType_STDDEVLO
transmissionMode_STDDEVLO $OutputSection_STDDEVLO.transmissionMode_STDDEVLO
outputType_STDDEVHI $OutputSection_STDDEVHI.outputType_STDDEVHI
transmissionMode_STDDEVHI $OutputSection_STDDEVHI.transmissionMode_STDDEVHI
outputType_PCTL $OutputSection_PCTL.outputType_PCTL
transmissionMode_PCTL $OutputSection_PCTL.transmissionMode_PCTL]]></command>
