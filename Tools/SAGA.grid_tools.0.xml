<tool name="SAGA.grid_tools.0" id="saga_grid_tools_0" version="1.0.0">
  <description>Resampling</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayINPUT True  isArrayTARGET_TEMPLATE False  output_data_OUTPUT $output_data_OUTPUT  name SAGA.grid_tools.0 INPUT $INPUT
KEEP_TYPE $KEEP_TYPE
SCALE_UP $SCALE_UP
SCALE_DOWN $SCALE_DOWN
TARGET_DEFINITION $TARGET_DEFINITION
TARGET_USER_SIZE $TARGET_USER_SIZE
TARGET_USER_XMIN $TARGET_USER_XMIN
TARGET_USER_XMAX $TARGET_USER_XMAX
TARGET_USER_YMIN $TARGET_USER_YMIN
TARGET_USER_YMAX $TARGET_USER_YMAX
TARGET_USER_COLS $TARGET_USER_COLS
TARGET_USER_ROWS $TARGET_USER_ROWS
TARGET_USER_FITS $TARGET_USER_FITS
TARGET_TEMPLATE $TARGET_TEMPLATE
prefer $prefer
response $response
outputType_OUTPUT $OutputSection_OUTPUT.outputType_OUTPUT
transmissionMode_OUTPUT $OutputSection_OUTPUT.transmissionMode_OUTPUT]]></command>
  <inputs>
    <param name="INPUT" type="data" optional="false" label="Grids" help="Grids The following data types are allowed in the txt file:  png, tiff, x-ogc-aaigrid, x-ogc-envi" format="txt"/>
    <param name="KEEP_TYPE" type="select" optional="true" label="Preserve Data Type" help="Preserve Data Type">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="SCALE_UP" type="select" optional="true" label="Upscaling Method" help="Upscaling Method">
      <option value="B-Spline_Interpolation">B-Spline Interpolation</option>
      <option value="Bicubic_Spline_Interpolation">Bicubic Spline Interpolation</option>
      <option value="Bilinear_Interpolation">Bilinear Interpolation</option>
      <option value="Majority">Majority</option>
      <option value="Maximum_Value">Maximum Value</option>
      <option value="Mean_Value">Mean Value</option>
      <option selected="true" value="Mean_Value_(cell_area_weighted)">Mean Value (cell area weighted)</option>
      <option value="Minimum_Value">Minimum Value</option>
      <option value="Nearest_Neighbour">Nearest Neighbour</option>
    </param>
    <param name="SCALE_DOWN" type="select" optional="true" label="Downscaling Method" help="Downscaling Method">
      <option selected="true" value="B-Spline_Interpolation">B-Spline Interpolation</option>
      <option value="Bicubic_Spline_Interpolation">Bicubic Spline Interpolation</option>
      <option value="Bilinear_Interpolation">Bilinear Interpolation</option>
      <option value="Nearest_Neighbour">Nearest Neighbour</option>
    </param>
    <param name="TARGET_DEFINITION" type="select" optional="true" label="Target Grid System" help="Target Grid System">
      <option value="grid_or_grid_system">grid or grid system</option>
      <option selected="true" value="user_defined">user defined</option>
    </param>
    <param name="TARGET_USER_SIZE" type="float" value="1" optional="true" label="Cellsize" help="Cellsize"/>
    <param name="TARGET_USER_XMIN" type="float" value="0" optional="true" label="West" help="West"/>
    <param name="TARGET_USER_XMAX" type="float" value="100" optional="true" label="East" help="East"/>
    <param name="TARGET_USER_YMIN" type="float" value="0" optional="true" label="South" help="South"/>
    <param name="TARGET_USER_YMAX" type="float" value="100" optional="true" label="North" help="North"/>
    <param name="TARGET_USER_COLS" type="integer" value="100" optional="true" label="Columns" help="Number of cells in East-West direction."/>
    <param name="TARGET_USER_ROWS" type="integer" value="100" optional="true" label="Rows" help="Number of cells in North-South direction."/>
    <param name="TARGET_USER_FITS" type="select" optional="true" label="Fit" help="Fit">
      <option value="cells">cells</option>
      <option selected="true" value="nodes">nodes</option>
    </param>
    <param name="TARGET_TEMPLATE" type="data" optional="false" label="Target System" help="use this grid's system for output grids The following data types are allowed in the txt file:  png, tiff, x-ogc-aaigrid, x-ogc-envi" format="txt"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_OUTPUT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_OUTPUT" type="select" label="Resampled Grids" help="Resampled Grids">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_OUTPUT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_OUTPUT" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_OUTPUT" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_OUTPUT" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_OUTPUT" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Resampling of grids.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
