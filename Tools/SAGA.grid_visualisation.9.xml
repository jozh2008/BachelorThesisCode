<tool name="SAGA.grid_visualisation.9" id="saga_grid_visualisation_9" version="1.0.0">
  <description>Split RGB Composite</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayRGB False  output_data_R $output_data_R  output_data_G $output_data_G  output_data_B $output_data_B  output_data_A $output_data_A  name SAGA.grid_visualisation.9 RGB $RGB
NODATA $NODATA
prefer $prefer
response $response
outputType_R $OutputSection_R.outputType_R
transmissionMode_R $OutputSection_R.transmissionMode_R
outputType_G $OutputSection_G.outputType_G
transmissionMode_G $OutputSection_G.transmissionMode_G
outputType_B $OutputSection_B.outputType_B
transmissionMode_B $OutputSection_B.transmissionMode_B
outputType_A $OutputSection_A.outputType_A
transmissionMode_A $OutputSection_A.transmissionMode_A]]></command>
  <inputs>
    <param name="RGB" type="data" optional="false" label="RGB Composite" help="RGB Composite The following data types are allowed in the txt file:  x-ogc-envi, x-ogc-aaigrid, tiff, png" format="txt"/>
    <param name="NODATA" type="select" optional="true" label="Ignore No Data" help="Ignore No Data">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_R" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_R" type="select" label="Red" help="Red">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_R" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_G" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_G" type="select" label="Green" help="Green">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_G" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_B" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_B" type="select" label="Blue" help="Blue">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_B" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_A" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_A" type="select" label="Alpha" help="Alpha">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_A" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_R" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_R" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_R" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_R" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_G" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_G" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_G" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_G" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_B" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_B" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_B" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_B" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_A" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_A" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_A" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_A" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Split red-green-blue channels of an rgb coded grid. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
