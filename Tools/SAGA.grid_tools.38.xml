<tool name="SAGA.grid_tools.38" id="saga_grid_tools_38" version="1.0.0">
  <description>Mosaicking (Grid Collections)</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayGRIDS True  isArrayTARGET_TEMPLATE False  output_data_MOSAIC $output_data_MOSAIC  name SAGA.grid_tools.38 GRIDS $GRIDS
NAME '$NAME'
TYPE $TYPE
RESAMPLING $RESAMPLING
OVERLAP $OVERLAP
BLEND_DIST $BLEND_DIST
BLEND_BND $BLEND_BND
MATCH $MATCH
TARGET_DEFINITION $TARGET_DEFINITION
TARGET_USER_SIZE $TARGET_USER_SIZE
TARGET_USER_XMIN $TARGET_USER_XMIN
TARGET_USER_XMAX $TARGET_USER_XMAX
TARGET_USER_YMIN $TARGET_USER_YMIN
TARGET_USER_YMAX $TARGET_USER_YMAX
TARGET_USER_COLS $TARGET_USER_COLS
TARGET_USER_ROWS $TARGET_USER_ROWS
TARGET_USER_FITS $TARGET_USER_FITS
TARGET_TEMPLATE $TARGET_TEMPLATE
prefer $prefer
response $response
outputType_MOSAIC $OutputSection_MOSAIC.outputType_MOSAIC
transmissionMode_MOSAIC $OutputSection_MOSAIC.transmissionMode_MOSAIC]]></command>
  <inputs>
    <param name="GRIDS" type="data" optional="false" label="Grid Collections" help="Grid Collections The following data types are allowed in the txt file:  x-ogc-envi, tiff, x-ogc-aaigrid, png" format="txt"/>
    <param name="NAME" type="text" value="Mosaic" optional="true" label="Name" help="Name"/>
    <param name="TYPE" type="select" optional="true" label="Data Storage Type" help="Data Storage Type">
      <option value="1_bit">1 bit</option>
      <option value="1_byte_signed_integer">1 byte signed integer</option>
      <option value="1_byte_unsigned_integer">1 byte unsigned integer</option>
      <option value="2_byte_signed_integer">2 byte signed integer</option>
      <option value="2_byte_unsigned_integer">2 byte unsigned integer</option>
      <option value="4_byte_floating_point">4 byte floating point</option>
      <option value="4_byte_signed_integer">4 byte signed integer</option>
      <option value="4_byte_unsigned_integer">4 byte unsigned integer</option>
      <option value="8_byte_floating_point">8 byte floating point</option>
      <option selected="true" value="same_as_first_grid_in_list">same as first grid in list</option>
    </param>
    <param name="RESAMPLING" type="select" optional="true" label="Resampling" help="Resampling">
      <option selected="true" value="B-Spline_Interpolation">B-Spline Interpolation</option>
      <option value="Bicubic_Spline_Interpolation">Bicubic Spline Interpolation</option>
      <option value="Bilinear_Interpolation">Bilinear Interpolation</option>
      <option value="Nearest_Neighbour">Nearest Neighbour</option>
    </param>
    <param name="OVERLAP" type="select" optional="true" label="Overlapping Areas" help="Overlapping Areas">
      <option value="blend_boundary">blend boundary</option>
      <option value="feathering">feathering</option>
      <option value="first">first</option>
      <option selected="true" value="last">last</option>
      <option value="maximum">maximum</option>
      <option value="mean">mean</option>
      <option value="minimum">minimum</option>
    </param>
    <param name="BLEND_DIST" type="float" value="10" optional="true" label="Blending Distance" help="blending distance given in map units"/>
    <param name="BLEND_BND" type="select" optional="true" label="Blending Boundary" help="blending boundary for distance calculation">
      <option value="grid_boundaries">grid boundaries</option>
      <option value="horizontal_grid_boundaries">horizontal grid boundaries</option>
      <option selected="true" value="valid_data_cells">valid data cells</option>
      <option value="vertical_grid_boundaries">vertical grid boundaries</option>
    </param>
    <param name="MATCH" type="select" optional="true" label="Match" help="Match">
      <option value="match_histogram_of_first_grid_in_list">match histogram of first grid in list</option>
      <option value="match_histogram_of_overlapping_area">match histogram of overlapping area</option>
      <option selected="true" value="none">none</option>
      <option value="regression">regression</option>
    </param>
    <param name="TARGET_DEFINITION" type="select" optional="true" label="Target Grid System" help="Target Grid System">
      <option value="grid_or_grid_system">grid or grid system</option>
      <option selected="true" value="user_defined">user defined</option>
    </param>
    <param name="TARGET_USER_SIZE" type="float" value="1" optional="true" label="Cellsize" help="Cellsize"/>
    <param name="TARGET_USER_XMIN" type="float" value="0" optional="true" label="West" help="West"/>
    <param name="TARGET_USER_XMAX" type="float" value="100" optional="true" label="East" help="East"/>
    <param name="TARGET_USER_YMIN" type="float" value="0" optional="true" label="South" help="South"/>
    <param name="TARGET_USER_YMAX" type="float" value="100" optional="true" label="North" help="North"/>
    <param name="TARGET_USER_COLS" type="integer" value="100" optional="true" label="Columns" help="Number of cells in East-West direction."/>
    <param name="TARGET_USER_ROWS" type="integer" value="100" optional="true" label="Rows" help="Number of cells in North-South direction."/>
    <param name="TARGET_USER_FITS" type="select" optional="true" label="Fit" help="Fit">
      <option value="cells">cells</option>
      <option selected="true" value="nodes">nodes</option>
    </param>
    <param name="TARGET_TEMPLATE" type="data" optional="false" label="Target System" help="use this grid's system for output grids The following data types are allowed in the txt file:  x-ogc-envi, tiff, x-ogc-aaigrid, png" format="txt"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_MOSAIC" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_MOSAIC" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_MOSAIC" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_MOSAIC" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_MOSAIC" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_MOSAIC" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_MOSAIC" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Merges multiple grid collections into one single grid collection. Input grid collections have to share the same number of grid levels. Attributes and other general properties will be inherited from the first grid collection in input list. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
