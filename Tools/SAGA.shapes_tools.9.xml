<tool name="SAGA.shapes_tools.9" id="saga_shapes_tools_9" version="1.0.0">
  <description>Split Shapes Layer Completely</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArraySHAPES False  isArrayFIELD False  output_data_LIST $output_data_LIST  name SAGA.shapes_tools.9 SHAPES $SHAPES
FIELD $FIELD
NAMING $NAMING
prefer $prefer
response $response
outputType_LIST $OutputSection_LIST.outputType_LIST
transmissionMode_LIST $OutputSection_LIST.transmissionMode_LIST]]></command>
  <inputs>
    <param name="SHAPES" type="data" optional="false" label="Input" help="Input The following data types are allowed in the txt file:  json, xml, vnd.google-earth.kml+xml" format="txt"/>
    <param name="FIELD" type="data" optional="false" label="Attribute" help="Attribute The following data types are allowed in the txt file:  csv" format="txt"/>
    <param name="NAMING" type="select" optional="true" label="Name by..." help="Name by...">
      <option value="attribute">attribute</option>
      <option selected="true" value="number_of_order">number of order</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_LIST" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_LIST" type="select" label="Output" help="Output">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_LIST" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_LIST" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_LIST" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_LIST" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Copies each shape of given layer to a separate target layer.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
