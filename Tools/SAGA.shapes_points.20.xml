<tool name="SAGA.shapes_points.20" id="saga_shapes_points_20" version="1.0.0">
  <description>Snap Points to Grid</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayINPUT False  isArrayGRID False  output_data_OUTPUT $output_data_OUTPUT  output_data_MOVES $output_data_MOVES  name SAGA.shapes_points.20 INPUT $INPUT
GRID $GRID
DISTANCE $DISTANCE
SHAPE $SHAPE
EXTREME $EXTREME
prefer $prefer
response $response
outputType_OUTPUT $OutputSection_OUTPUT.outputType_OUTPUT
transmissionMode_OUTPUT $OutputSection_OUTPUT.transmissionMode_OUTPUT
outputType_MOVES $OutputSection_MOVES.outputType_MOVES
transmissionMode_MOVES $OutputSection_MOVES.transmissionMode_MOVES]]></command>
  <inputs>
    <param name="INPUT" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  json, vnd.google-earth.kml+xml, xml" format="txt"/>
    <param name="GRID" type="data" optional="false" label="Grid" help="Grid The following data types are allowed in the txt file:  tiff, x-ogc-aaigrid, png, x-ogc-envi" format="txt"/>
    <param name="DISTANCE" type="float" value="0" optional="true" label="Search Distance (Map Units)" help="Search Distance (Map Units)"/>
    <param name="SHAPE" type="select" optional="true" label="Search Shape" help="Search Shape">
      <option selected="true" value="circle">circle</option>
      <option value="square">square</option>
    </param>
    <param name="EXTREME" type="select" optional="true" label="Extreme" help="Extreme">
      <option selected="true" value="maximum">maximum</option>
      <option value="minimum">minimum</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_OUTPUT" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_OUTPUT" type="select" label="Result" help="Result">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_OUTPUT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_MOVES" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_MOVES" type="select" label="Moves" help="Moves">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_MOVES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_OUTPUT" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_OUTPUT" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_OUTPUT" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_MOVES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_MOVES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_MOVES" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Moves all points to grid cell positions that have the highest orlowest value respectively within the given search distance around each point.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
