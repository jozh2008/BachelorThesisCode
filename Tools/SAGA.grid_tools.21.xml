<tool name="SAGA.grid_tools.21" id="saga_grid_tools_21" version="1.0.0">
  <description>Grid Cell Index</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayGRID False  output_data_INDEX $output_data_INDEX  name SAGA.grid_tools.21 GRID $GRID
ORDER $ORDER
prefer $prefer
response $response
outputType_INDEX $OutputSection_INDEX.outputType_INDEX
transmissionMode_INDEX $OutputSection_INDEX.transmissionMode_INDEX]]></command>
  <inputs>
    <param name="GRID" type="data" optional="false" label="Grid" help="Grid The following data types are allowed in the txt file:  x-ogc-aaigrid, tiff, x-ogc-envi, png" format="txt"/>
    <param name="ORDER" type="select" optional="true" label="Sorting Order" help="Sorting Order">
      <option selected="true" value="ascending">ascending</option>
      <option value="descending">descending</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_INDEX" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_INDEX" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_INDEX" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_INDEX" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_INDEX" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_INDEX" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_INDEX" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Creates an index grid according to the cell values either in ascending or descending order.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
