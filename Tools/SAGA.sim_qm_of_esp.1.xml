<tool name="SAGA.sim_qm_of_esp.1" id="saga_sim_qm_of_esp_1" version="1.0.0">
  <description>Fill Sinks (QM of ESP)</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayDEM False  output_data_FILLED $output_data_FILLED  output_data_SINKS $output_data_SINKS  name SAGA.sim_qm_of_esp.1 DEM $DEM
DZFILL $DZFILL
prefer $prefer
response $response
outputType_FILLED $OutputSection_FILLED.outputType_FILLED
transmissionMode_FILLED $OutputSection_FILLED.transmissionMode_FILLED
outputType_SINKS $OutputSection_SINKS.outputType_SINKS
transmissionMode_SINKS $OutputSection_SINKS.transmissionMode_SINKS]]></command>
  <inputs>
    <param name="DEM" type="data" optional="false" label="DEM" help="DEM The following data types are allowed in the txt file:  x-ogc-envi, x-ogc-aaigrid, tiff, png" format="txt"/>
    <param name="DZFILL" type="float" value="0.01" optional="true" label="Fill Increment" help="Fill Increment"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_FILLED" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_FILLED" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_FILLED" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_SINKS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_SINKS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_SINKS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_FILLED" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_FILLED" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_FILLED" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_FILLED" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_SINKS" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SINKS" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_SINKS" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_SINKS" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Filling in pits and flats in a DEM.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
