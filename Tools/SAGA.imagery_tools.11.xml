outputType_IDM $OutputSection_IDM.outputType_IDM
transmissionMode_IDM $OutputSection_IDM.transmissionMode_IDM
outputType_SUM_AVERAGE $OutputSection_SUM_AVERAGE.outputType_SUM_AVERAGE
transmissionMode_SUM_AVERAGE $OutputSection_SUM_AVERAGE.transmissionMode_SUM_AVERAGE
