<tool name="SAGA.garden_fractals.0" id="saga_garden_fractals_0" version="1.0.0">
  <description>Bifurcation</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py output_data_TABLE $output_data_TABLE  name SAGA.garden_fractals.0 ITERATIONS $ITERATIONS
NVALUES $NVALUES
SEED $SEED
RANGE_MIN $RANGE_MIN
RANGE_MAX $RANGE_MAX
STEP $STEP
prefer $prefer
response $response
outputType_TABLE $OutputSection_TABLE.outputType_TABLE
transmissionMode_TABLE $OutputSection_TABLE.transmissionMode_TABLE]]></command>
  <inputs>
    <param name="ITERATIONS" type="integer" value="100" optional="true" label="Value Count" help="Value Count"/>
    <param name="NVALUES" type="integer" value="8" optional="true" label="Value Count" help="Number of the last x Iteration to be plotted."/>
    <param name="SEED" type="float" value="0.66" optional="true" label="Seed Value" help="Seed Value"/>
    <param name="RANGE_MIN" type="float" optional="true" label="Range (min value)" help="Range"/>
    <param name="RANGE_MAX" type="float" optional="true" label="Range (max value)" help="Range"/>
    <param name="STEP" type="integer" value="1000" optional="true" label="Number of Steps" help="Number of Steps"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_TABLE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_TABLE" type="select" label="Author did not provide help for this parameter... ">
        <option value="text/csv">csv</option>
      </param>
      <param name="transmissionMode_TABLE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_TABLE" format="csv" label="output_data_TABLE" hidden="false"/>
  </outputs>
  <tests>
    <test>
      <param name="response" value="document"/>
      <output name="output_data_TABLE" ftype="txt" value="output_data_TABLE.txt"/>
    </test>
  </tests>
  <help><![CDATA[Feigenbaum's Bifurcation]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
