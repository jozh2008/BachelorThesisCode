<tool name="SAGA.ta_channels.2" id="saga_ta_channels_2" version="1.0.0">
  <description>Watershed Basins (Extended)</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayDEM False  isArrayCHANNELS False  output_data_BASINS $output_data_BASINS  output_data_SUBBASINS $output_data_SUBBASINS  output_data_V_BASINS $output_data_V_BASINS  output_data_V_SUBBASINS $output_data_V_SUBBASINS  output_data_HEADS $output_data_HEADS  output_data_MOUTHS $output_data_MOUTHS  name SAGA.ta_channels.2 DEM $DEM
CHANNELS $CHANNELS
DISTANCE $DISTANCE
prefer $prefer
response $response
outputType_BASINS $OutputSection_BASINS.outputType_BASINS
transmissionMode_BASINS $OutputSection_BASINS.transmissionMode_BASINS
outputType_SUBBASINS $OutputSection_SUBBASINS.outputType_SUBBASINS
transmissionMode_SUBBASINS $OutputSection_SUBBASINS.transmissionMode_SUBBASINS
outputType_V_BASINS $OutputSection_V_BASINS.outputType_V_BASINS
transmissionMode_V_BASINS $OutputSection_V_BASINS.transmissionMode_V_BASINS
outputType_V_SUBBASINS $OutputSection_V_SUBBASINS.outputType_V_SUBBASINS
transmissionMode_V_SUBBASINS $OutputSection_V_SUBBASINS.transmissionMode_V_SUBBASINS
outputType_HEADS $OutputSection_HEADS.outputType_HEADS
transmissionMode_HEADS $OutputSection_HEADS.transmissionMode_HEADS
outputType_MOUTHS $OutputSection_MOUTHS.outputType_MOUTHS
transmissionMode_MOUTHS $OutputSection_MOUTHS.transmissionMode_MOUTHS]]></command>
  <inputs>
    <param name="DEM" type="data" optional="false" label="DEM" help="DEM The following data types are allowed in the txt file:  x-ogc-aaigrid, png, x-ogc-envi, tiff" format="txt"/>
    <param name="CHANNELS" type="data" optional="false" label="Drainage Network" help="Drainage Network The following data types are allowed in the txt file:  x-ogc-aaigrid, png, x-ogc-envi, tiff" format="txt"/>
    <param name="DISTANCE" type="select" optional="true" label="Flow Distances" help="Flow Distances">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_BASINS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_BASINS" type="select" label="Basins" help="Basins">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_BASINS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_SUBBASINS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_SUBBASINS" type="select" label="Subbasins" help="Subbasins">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_SUBBASINS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_V_BASINS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_V_BASINS" type="select" label="Basins" help="Basins">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_V_BASINS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_V_SUBBASINS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_V_SUBBASINS" type="select" label="Subbasins" help="Subbasins">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_V_SUBBASINS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_HEADS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_HEADS" type="select" label="River Heads" help="River Heads">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_HEADS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_MOUTHS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_MOUTHS" type="select" label="River Mouths" help="River Mouths">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_MOUTHS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_BASINS" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_BASINS" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_BASINS" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_BASINS" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_SUBBASINS" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SUBBASINS" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_SUBBASINS" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_SUBBASINS" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_V_BASINS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_V_BASINS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_V_BASINS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_V_SUBBASINS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_V_SUBBASINS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_V_SUBBASINS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_HEADS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_HEADS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_HEADS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_MOUTHS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_MOUTHS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_MOUTHS" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Extended watershed basin analysis. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
