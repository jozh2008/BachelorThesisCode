<tool name="SAGA.shapes_points.6" id="saga_shapes_points_6" version="1.0.0">
  <description>Add Coordinates to Points</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayINPUT False  output_data_OUTPUT $output_data_OUTPUT  name SAGA.shapes_points.6 INPUT $INPUT
X $X
Y $Y
Z $Z
M $M
LON $LON
LAT $LAT
prefer $prefer
response $response
outputType_OUTPUT $OutputSection_OUTPUT.outputType_OUTPUT
transmissionMode_OUTPUT $OutputSection_OUTPUT.transmissionMode_OUTPUT]]></command>
  <inputs>
    <param name="INPUT" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="X" type="select" optional="true" label="X" help="X">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="Y" type="select" optional="true" label="Y" help="Y">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="Z" type="select" optional="true" label="Z" help="Z">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="M" type="select" optional="true" label="M" help="M">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="LON" type="select" optional="true" label="Longitude" help="Longitude">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="LAT" type="select" optional="true" label="Latitude" help="Latitude">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_OUTPUT" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_OUTPUT" type="select" label="Output" help="Output">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_OUTPUT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_OUTPUT" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_OUTPUT" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_OUTPUT" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[The tool attaches the x- and y-coordinates of each point to the attribute table. For 3D shapefiles, also the z/m-coordinates are reported.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
