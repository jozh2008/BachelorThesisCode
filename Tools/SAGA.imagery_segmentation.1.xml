<tool name="SAGA.imagery_segmentation.1" id="saga_imagery_segmentation_1" version="1.0.0">
  <description>Grid Skeletonization</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayINPUT False  output_data_RESULT $output_data_RESULT  output_data_VECTOR $output_data_VECTOR  name SAGA.imagery_segmentation.1 INPUT $INPUT
METHOD $METHOD
INIT_METHOD $INIT_METHOD
INIT_THRESHOLD $INIT_THRESHOLD
CONVERGENCE $CONVERGENCE
prefer $prefer
response $response
outputType_RESULT $OutputSection_RESULT.outputType_RESULT
transmissionMode_RESULT $OutputSection_RESULT.transmissionMode_RESULT
outputType_VECTOR $OutputSection_VECTOR.outputType_VECTOR
transmissionMode_VECTOR $OutputSection_VECTOR.transmissionMode_VECTOR]]></command>
  <inputs>
    <param name="INPUT" type="data" optional="false" label="Grid" help="Grid The following data types are allowed in the txt file:  png, x-ogc-aaigrid, tiff, x-ogc-envi" format="txt"/>
    <param name="METHOD" type="select" optional="true" label="Method" help="Method">
      <option value="Channel_Skeleton">Channel Skeleton</option>
      <option value="Hilditch's_Algorithm">Hilditch's Algorithm</option>
      <option selected="true" value="Standard">Standard</option>
    </param>
    <param name="INIT_METHOD" type="select" optional="true" label="Initialisation" help="Initialisation">
      <option selected="true" value="Greater_than">Greater than</option>
      <option value="Less_than">Less than</option>
    </param>
    <param name="INIT_THRESHOLD" type="float" value="0" optional="true" label="Threshold (Init.)" help="Threshold (Init.)"/>
    <param name="CONVERGENCE" type="integer" value="3" optional="true" label="Convergence" help="Convergence"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_RESULT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_RESULT" type="select" label="Skeleton" help="Skeleton">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_RESULT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_VECTOR" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_VECTOR" type="select" label="Skeleton" help="Skeleton">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_VECTOR" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_RESULT" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_RESULT" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_RESULT" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_RESULT" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_VECTOR" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_VECTOR" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_VECTOR" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Simple skeletonisation methods for grids. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
