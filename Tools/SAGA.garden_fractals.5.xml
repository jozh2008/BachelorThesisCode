<tool name="SAGA.garden_fractals.5" id="saga_garden_fractals_5" version="1.0.0">
  <description>Gaussian Landscapes</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py output_data_GRID $output_data_GRID  name SAGA.garden_fractals.5 NX $NX
NY $NY
H $H
METHOD $METHOD
M $M
prefer $prefer
response $response
outputType_GRID $OutputSection_GRID.outputType_GRID
transmissionMode_GRID $OutputSection_GRID.transmissionMode_GRID]]></command>
  <inputs>
    <param name="NX" type="integer" value="100" optional="true" label="Width (Cells)" help="Width (Cells)"/>
    <param name="NY" type="integer" value="100" optional="true" label="Height (Cells)" help="Height (Cells)"/>
    <param name="H" type="float" value="0.75" optional="true" label="Roughness/Smoothness" help="Roughness/Smoothness"/>
    <param name="METHOD" type="select" optional="true" label="Method" help="Method">
      <option value="Flattening">Flattening</option>
      <option selected="true" value="Simple">Simple</option>
    </param>
    <param name="M" type="float" value="2" optional="true" label="Flattening" help="Flattening"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_GRID" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_GRID" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_GRID" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_GRID" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRID" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_GRID" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_GRID" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Generates Gaussian landscapes.&lt;br/&gt;&lt;br/&gt;References:&lt;br/&gt;- Halling, H., Moeller, R. (1995): 'Mathematik fuers Auge', Heidelberg, 144p.&lt;br/&gt;- Mandelbrot, B.B. (1983): 'The Fractal Geometry of Nature', New York, 490p.&lt;br/&gt;]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
