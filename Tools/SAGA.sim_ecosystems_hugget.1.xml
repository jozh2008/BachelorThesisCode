<tool name="SAGA.sim_ecosystems_hugget.1" id="saga_sim_ecosystems_hugget_1" version="1.0.0">
  <description>02: Carbon Cycle Simulation for Terrestrial Biomass</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py output_data_TABLE $output_data_TABLE  name SAGA.sim_ecosystems_hugget.1 TIME_SPAN $TIME_SPAN
TIME_STEP $TIME_STEP
PRIMPROD $PRIMPROD
CHUMIFY $CHUMIFY
CCARBON $CCARBON
P_LEAV $P_LEAV
P_BRAN $P_BRAN
P_STEM $P_STEM
P_ROOT $P_ROOT
K_LEAV_LITT $K_LEAV_LITT
K_BRAN_LITT $K_BRAN_LITT
K_STEM_LITT $K_STEM_LITT
K_ROOT_HUMU $K_ROOT_HUMU
K_LITT_HUMU $K_LITT_HUMU
K_HUMU_COAL $K_HUMU_COAL
K_COAL_ENVI $K_COAL_ENVI
prefer $prefer
response $response
outputType_TABLE $OutputSection_TABLE.outputType_TABLE
transmissionMode_TABLE $OutputSection_TABLE.transmissionMode_TABLE]]></command>
  <inputs>
    <param name="TIME_SPAN" type="float" value="100" optional="true" label="Time Span" help="Time Span"/>
    <param name="TIME_STEP" type="float" value="0.1" optional="true" label="Time Interval [a]" help="Time Interval [a]"/>
    <param name="PRIMPROD" type="float" value="27.8" optional="true" label="Net Primary Production [Gt Carbon / a]" help="Net Primary Production [Gt Carbon / a]"/>
    <param name="CHUMIFY" type="float" value="0.4" optional="true" label="Humification Factor" help="Humification Factor"/>
    <param name="CCARBON" type="float" value="0.05" optional="true" label="Carbonization Factor" help="Carbonization Factor"/>
    <param name="P_LEAV" type="float" value="0.3" optional="true" label="Leaves" help="Leaves"/>
    <param name="P_BRAN" type="float" value="0.2" optional="true" label="Branches" help="Branches"/>
    <param name="P_STEM" type="float" value="0.3" optional="true" label="Stems" help="Stems"/>
    <param name="P_ROOT" type="float" value="0.2" optional="true" label="Roots" help="Roots"/>
    <param name="K_LEAV_LITT" type="float" value="1" optional="true" label="Leaves to Litter" help="Leaves to Litter"/>
    <param name="K_BRAN_LITT" type="float" value="0.1" optional="true" label="Branches to Litter" help="Branches to Litter"/>
    <param name="K_STEM_LITT" type="float" value="0.033" optional="true" label="Stems to Litter" help="Stems to Litter"/>
    <param name="K_ROOT_HUMU" type="float" value="0.1" optional="true" label="Roots to Humus" help="Roots to Humus"/>
    <param name="K_LITT_HUMU" type="float" value="1" optional="true" label="Litter to Humus" help="Litter to Humus"/>
    <param name="K_HUMU_COAL" type="float" value="0.1" optional="true" label="Humus to Charcoal" help="Humus to Charcoal"/>
    <param name="K_COAL_ENVI" type="float" value="0.002" optional="true" label="Charcoal to Environment" help="Charcoal to Environment"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_TABLE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_TABLE" type="select" label="Author did not provide help for this parameter... ">
        <option value="text/csv">csv</option>
      </param>
      <param name="transmissionMode_TABLE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_TABLE" format="csv" hidden="false"/>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Simulation of the Carbon Cycle in Terrestrial Biomass. &lt;br/&gt;Reference:&lt;br/&gt;Hugget, R.J. (1993): 'Modelling the Human Impact on Nature', Oxford University Press.&lt;br/&gt;]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
