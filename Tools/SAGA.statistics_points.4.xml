<tool name="SAGA.statistics_points.4" id="saga_statistics_points_4" version="1.0.0">
  <description>Spatial Point Pattern Analysis</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOINTS False  isArrayWEIGHT False  output_data_CENTRE $output_data_CENTRE  output_data_STDDIST $output_data_STDDIST  output_data_BBOX $output_data_BBOX  name SAGA.statistics_points.4 POINTS $POINTS
WEIGHT $WEIGHT
STEP $STEP
prefer $prefer
response $response
outputType_CENTRE $OutputSection_CENTRE.outputType_CENTRE
transmissionMode_CENTRE $OutputSection_CENTRE.transmissionMode_CENTRE
outputType_STDDIST $OutputSection_STDDIST.outputType_STDDIST
transmissionMode_STDDIST $OutputSection_STDDIST.transmissionMode_STDDIST
outputType_BBOX $OutputSection_BBOX.outputType_BBOX
transmissionMode_BBOX $OutputSection_BBOX.transmissionMode_BBOX]]></command>
  <inputs>
    <param name="POINTS" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  json, xml, vnd.google-earth.kml+xml" format="txt"/>
    <param name="WEIGHT" type="data" optional="false" label="Weight" help="Weight The following data types are allowed in the txt file:  csv" format="txt"/>
    <param name="STEP" type="float" value="5" optional="true" label="Vertex Distance [Degree]" help="Vertex Distance [Degree]"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_CENTRE" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CENTRE" type="select" label="Mean Centre" help="Mean Centre">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_CENTRE" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_STDDIST" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_STDDIST" type="select" label="Standard Distance" help="Standard Distance">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_STDDIST" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_BBOX" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_BBOX" type="select" label="Bounding Box" help="Bounding Box">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_BBOX" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_CENTRE" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_CENTRE" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_CENTRE" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_STDDIST" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_STDDIST" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_STDDIST" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_BBOX" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_BBOX" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_BBOX" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Basic measures for spatial point patterns.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
