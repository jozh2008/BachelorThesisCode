<tool name="SAGA.shapes_tools.21" id="saga_shapes_tools_21" version="1.0.0">
  <description>Polar to Cartesian Coordinates</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOLAR False  isArrayF_EXAGG False  output_data_CARTES $output_data_CARTES  name SAGA.shapes_tools.21 POLAR $POLAR
F_EXAGG $F_EXAGG
D_EXAGG $D_EXAGG
RADIUS $RADIUS
DEGREE $DEGREE
prefer $prefer
response $response
outputType_CARTES $OutputSection_CARTES.outputType_CARTES
transmissionMode_CARTES $OutputSection_CARTES.transmissionMode_CARTES]]></command>
  <inputs>
    <param name="POLAR" type="data" optional="false" label="Polar Coordinates" help="Polar Coordinates The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="F_EXAGG" type="data" optional="false" label="Exaggeration" help="Exaggeration The following data types are allowed in the txt file:  csv" format="txt"/>
    <param name="D_EXAGG" type="float" value="1" optional="true" label="Exaggeration Factor" help="Exaggeration Factor"/>
    <param name="RADIUS" type="float" value="6371000" optional="true" label="Radius" help="Radius"/>
    <param name="DEGREE" type="select" optional="true" label="Degree" help="polar coordinates given in degree">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_CARTES" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CARTES" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_CARTES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_CARTES" format="xml" label="output_data_CARTES" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_CARTES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_CARTES" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <param name="response" value="document"/>
      <output name="output_data_CARTES" ftype="txt" value="output_data_CARTES.txt"/>
    </test>
  </tests>
  <help><![CDATA[Polar to Cartesian Coordinates]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
