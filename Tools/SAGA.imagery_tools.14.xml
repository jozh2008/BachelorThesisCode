<tool name="SAGA.imagery_tools.14" id="saga_imagery_tools_14" version="1.0.0">
  <description>Import Landsat Scene</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayMETAFILE False  output_data_BANDS $output_data_BANDS  output_data_BAND_INFO $output_data_BAND_INFO  name SAGA.imagery_tools.14 METAFILE $METAFILE
MULTI2GRIDS $MULTI2GRIDS
SKIP_PAN $SKIP_PAN
CALIBRATION $CALIBRATION
DATA_TYPE $DATA_TYPE
TEMP_UNIT $TEMP_UNIT
PROJECTION $PROJECTION
RESAMPLING $RESAMPLING
prefer $prefer
response $response
outputType_BANDS $OutputSection_BANDS.outputType_BANDS
transmissionMode_BANDS $OutputSection_BANDS.transmissionMode_BANDS
outputType_BAND_INFO $OutputSection_BAND_INFO.outputType_BAND_INFO
transmissionMode_BAND_INFO $OutputSection_BAND_INFO.transmissionMode_BAND_INFO]]></command>
  <inputs>
    <param name="METAFILE" type="data" optional="false" label="Metadata File" help="Metadata File The following data types are allowed in the txt file:  plain" format="txt"/>
    <param name="MULTI2GRIDS" type="select" optional="true" label="Multispectral Bands as Grid Collection" help="Multispectral Bands as Grid Collection">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="SKIP_PAN" type="select" optional="true" label="Skip Panchromatic Band" help="Skip Panchromatic Band">
      <option selected="true" value="false">false</option>
      <option value="true">true</option>
    </param>
    <param name="CALIBRATION" type="select" optional="true" label="Radiometric Calibration" help="Radiometric Calibration">
      <option selected="true" value="none">none</option>
      <option value="radiance">radiance</option>
      <option value="reflectance">reflectance</option>
    </param>
    <param name="DATA_TYPE" type="select" optional="true" label="Output Data Type" help="Output Data Type">
      <option value="floating_point_numbers">floating point numbers</option>
      <option selected="true" value="integers_with_scaling">integers with scaling</option>
    </param>
    <param name="TEMP_UNIT" type="select" optional="true" label="Temperature Unit" help="Temperature Unit">
      <option value="Celsius">Celsius</option>
      <option selected="true" value="Kelvin">Kelvin</option>
    </param>
    <param name="PROJECTION" type="select" optional="true" label="Coordinate System" help="Coordinate System">
      <option value="Geographic_Coordinates">Geographic Coordinates</option>
      <option selected="true" value="UTM_North">UTM North</option>
      <option value="UTM_South">UTM South</option>
    </param>
    <param name="RESAMPLING" type="select" optional="true" label="Resampling" help="Resampling">
      <option selected="true" value="B-Spline_Interpolation">B-Spline Interpolation</option>
      <option value="Bicubic_Spline_Interpolation">Bicubic Spline Interpolation</option>
      <option value="Bilinear_Interpolation">Bilinear Interpolation</option>
      <option value="Nearest_Neighbour">Nearest Neighbour</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_BANDS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_BANDS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_BANDS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_BAND_INFO" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_BAND_INFO" type="select" label="Author did not provide help for this parameter... ">
        <option value="text/csv">csv</option>
      </param>
      <param name="transmissionMode_BAND_INFO" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_BANDS" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_BANDS" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_BANDS" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_BANDS" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_BAND_INFO" format="csv" hidden="false"/>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Import Landsat scenes including metadata from Landsat metadata files. Band data have to be stored in the same folder as the chosen metadata file in uncompressed GeoTIFF format. ]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
