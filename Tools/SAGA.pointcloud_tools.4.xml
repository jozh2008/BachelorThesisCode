<tool name="SAGA.pointcloud_tools.4" id="saga_pointcloud_tools_4" version="1.0.0">
  <description>Point Cloud to Grid</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOINTS False  output_data_GRID $output_data_GRID  output_data_COUNT $output_data_COUNT  output_data_GRIDS $output_data_GRIDS  name SAGA.pointcloud_tools.4 POINTS $POINTS
OUTPUT $OUTPUT
AGGREGATION $AGGREGATION
CELLSIZE $CELLSIZE
prefer $prefer
response $response
outputType_GRID $OutputSection_GRID.outputType_GRID
transmissionMode_GRID $OutputSection_GRID.transmissionMode_GRID
outputType_COUNT $OutputSection_COUNT.outputType_COUNT
transmissionMode_COUNT $OutputSection_COUNT.transmissionMode_COUNT
outputType_GRIDS $OutputSection_GRIDS.outputType_GRIDS
transmissionMode_GRIDS $OutputSection_GRIDS.transmissionMode_GRIDS]]></command>
  <inputs>
    <param name="POINTS" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  x-ogc-lasf" format="txt"/>
    <param name="OUTPUT" type="select" optional="true" label="Output" help="Output">
      <option value="all_attributes">all attributes</option>
      <option selected="true" value="only_z">only z</option>
    </param>
    <param name="AGGREGATION" type="select" optional="true" label="Aggregation" help="Aggregation">
      <option selected="true" value="first_value">first value</option>
      <option value="highest_z">highest z</option>
      <option value="last_value">last value</option>
      <option value="lowest_z">lowest z</option>
      <option value="mean_value">mean value</option>
    </param>
    <param name="CELLSIZE" type="float" value="1" optional="true" label="Cellsize" help="Cellsize"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_GRID" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_GRID" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_GRID" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_COUNT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_COUNT" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_COUNT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_GRIDS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_GRIDS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_GRIDS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_GRID" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRID" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_GRID" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_GRID" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_COUNT" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_COUNT" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_COUNT" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_COUNT" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_GRIDS" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_GRIDS" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_GRIDS" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_GRIDS" format="png" value="image/png"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Point Cloud to Grid]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
