<tool name="SAGA.ta_channels.5" id="saga_ta_channels_5" version="1.0.0">
  <description>Channel Network and Drainage Basins</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayDEM False  output_data_DIRECTION $output_data_DIRECTION  output_data_CONNECTION $output_data_CONNECTION  output_data_ORDER $output_data_ORDER  output_data_BASIN $output_data_BASIN  output_data_SEGMENTS $output_data_SEGMENTS  output_data_BASINS $output_data_BASINS  output_data_NODES $output_data_NODES  name SAGA.ta_channels.5 DEM $DEM
THRESHOLD $THRESHOLD
prefer $prefer
response $response
outputType_DIRECTION $OutputSection_DIRECTION.outputType_DIRECTION
transmissionMode_DIRECTION $OutputSection_DIRECTION.transmissionMode_DIRECTION
outputType_CONNECTION $OutputSection_CONNECTION.outputType_CONNECTION
transmissionMode_CONNECTION $OutputSection_CONNECTION.transmissionMode_CONNECTION
outputType_ORDER $OutputSection_ORDER.outputType_ORDER
transmissionMode_ORDER $OutputSection_ORDER.transmissionMode_ORDER
outputType_BASIN $OutputSection_BASIN.outputType_BASIN
transmissionMode_BASIN $OutputSection_BASIN.transmissionMode_BASIN
outputType_SEGMENTS $OutputSection_SEGMENTS.outputType_SEGMENTS
transmissionMode_SEGMENTS $OutputSection_SEGMENTS.transmissionMode_SEGMENTS
outputType_BASINS $OutputSection_BASINS.outputType_BASINS
transmissionMode_BASINS $OutputSection_BASINS.transmissionMode_BASINS
outputType_NODES $OutputSection_NODES.outputType_NODES
transmissionMode_NODES $OutputSection_NODES.transmissionMode_NODES]]></command>
  <inputs>
    <param name="DEM" type="data" optional="false" label="Elevation" help="Elevation The following data types are allowed in the txt file:  x-ogc-envi, x-ogc-aaigrid, tiff, png" format="txt"/>
    <param name="THRESHOLD" type="integer" value="5" optional="true" label="Threshold" help="Strahler order to begin a channel."/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_DIRECTION" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_DIRECTION" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_DIRECTION" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_CONNECTION" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_CONNECTION" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_CONNECTION" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_ORDER" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_ORDER" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_ORDER" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_BASIN" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_BASIN" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/x-ogc-aaigrid">x-ogc-aaigrid</option>
        <option value="application/x-ogc-envi">x-ogc-envi</option>
        <option value="image/png">png</option>
        <option value="image/tiff">tiff</option>
      </param>
      <param name="transmissionMode_BASIN" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_SEGMENTS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_SEGMENTS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_SEGMENTS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_BASINS" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_BASINS" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_BASINS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_NODES" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_NODES" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_NODES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_DIRECTION" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_DIRECTION" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_DIRECTION" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_DIRECTION" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_CONNECTION" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_CONNECTION" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_CONNECTION" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_CONNECTION" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_ORDER" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_ORDER" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_ORDER" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_ORDER" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_BASIN" format="tiff" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_BASIN" format="x-ogc-envi" value="application/x-ogc-envi"/>
        <when input="outputType_BASIN" format="x-ogc-aaigrid" value="application/x-ogc-aaigrid"/>
        <when input="outputType_BASIN" format="png" value="image/png"/>
      </change_format>
    </data>
    <data name="output_data_SEGMENTS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SEGMENTS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_SEGMENTS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_BASINS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_BASINS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_BASINS" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_NODES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_NODES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_NODES" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Deterministic 8 based flow network analysis&lt;br/&gt;]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
