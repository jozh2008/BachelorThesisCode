<tool name="SAGA.shapes_points.7" id="saga_shapes_points_7" version="1.0.0">
  <description>Remove Duplicate Points</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOINTS False  isArrayFIELD False  output_data_RESULT $output_data_RESULT  name SAGA.shapes_points.7 POINTS $POINTS
FIELD $FIELD
NUMERIC $NUMERIC
METHOD $METHOD
prefer $prefer
response $response
outputType_RESULT $OutputSection_RESULT.outputType_RESULT
transmissionMode_RESULT $OutputSection_RESULT.transmissionMode_RESULT]]></command>
  <inputs>
    <param name="POINTS" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="FIELD" type="data" optional="false" label="Attribute" help="Attribute The following data types are allowed in the txt file:  csv" format="txt"/>
    <param name="NUMERIC" type="select" optional="true" label="Value Aggregation" help="Value Aggregation">
      <option value="maximum_values_of_all_duplicates">maximum values of all duplicates</option>
      <option value="mean_values_of_all_duplicates">mean values of all duplicates</option>
      <option value="minimum_values_of_all_duplicates">minimum values of all duplicates</option>
      <option selected="true" value="take_values_from_the_point_to_be_kept">take values from the point to be kept</option>
    </param>
    <param name="METHOD" type="select" optional="true" label="Point to Keep" help="Point to Keep">
      <option selected="true" value="first_point">first point</option>
      <option value="last_point">last point</option>
      <option value="point_with_maximum_attribute_value">point with maximum attribute value</option>
      <option value="point_with_minimum_attribute_value">point with minimum attribute value</option>
    </param>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_RESULT" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_RESULT" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_RESULT" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_RESULT" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_RESULT" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_RESULT" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Removes duplicate points.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
