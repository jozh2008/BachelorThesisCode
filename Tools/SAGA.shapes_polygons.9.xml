<tool name="SAGA.shapes_polygons.9" id="saga_shapes_polygons_9" version="1.0.0">
  <description>Polygons to Edges and Nodes</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOLYGONS False  output_data_EDGES $output_data_EDGES  output_data_NODES $output_data_NODES  name SAGA.shapes_polygons.9 POLYGONS $POLYGONS
prefer $prefer
response $response
outputType_EDGES $OutputSection_EDGES.outputType_EDGES
transmissionMode_EDGES $OutputSection_EDGES.transmissionMode_EDGES
outputType_NODES $OutputSection_NODES.outputType_NODES
transmissionMode_NODES $OutputSection_NODES.transmissionMode_NODES]]></command>
  <inputs>
    <param name="POLYGONS" type="data" optional="false" label="Polygons" help="Polygons The following data types are allowed in the txt file:  json, vnd.google-earth.kml+xml, xml" format="txt"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_EDGES" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_EDGES" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_EDGES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
    <section name="OutputSection_NODES" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_NODES" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_NODES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_EDGES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_EDGES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_EDGES" format="json" value="application/json"/>
      </change_format>
    </data>
    <data name="output_data_NODES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_NODES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_NODES" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Polygons to Edges and Nodes]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
