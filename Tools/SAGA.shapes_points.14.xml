<tool name="SAGA.shapes_points.14" id="saga_shapes_points_14" version="1.0.0">
  <description>Points Thinning</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOINTS False  isArrayFIELD False  output_data_THINNED $output_data_THINNED  name SAGA.shapes_points.14 POINTS $POINTS
FIELD $FIELD
RESOLUTION $RESOLUTION
prefer $prefer
response $response
outputType_THINNED $OutputSection_THINNED.outputType_THINNED
transmissionMode_THINNED $OutputSection_THINNED.transmissionMode_THINNED]]></command>
  <inputs>
    <param name="POINTS" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  xml, vnd.google-earth.kml+xml, json" format="txt"/>
    <param name="FIELD" type="data" optional="false" label="Attribute" help="Attribute The following data types are allowed in the txt file:  csv" format="txt"/>
    <param name="RESOLUTION" type="float" value="1" optional="true" label="Resolution" help="Resolution"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_THINNED" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_THINNED" type="select" label="Thinned Points" help="Thinned Points">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_THINNED" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_THINNED" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_THINNED" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_THINNED" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Points Thinning.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
