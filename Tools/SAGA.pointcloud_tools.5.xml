<tool name="SAGA.pointcloud_tools.5" id="saga_pointcloud_tools_5" version="1.0.0">
  <description>Point Cloud to Shapes</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArrayPOINTS False  output_data_SHAPES $output_data_SHAPES  name SAGA.pointcloud_tools.5 POINTS $POINTS
prefer $prefer
response $response
outputType_SHAPES $OutputSection_SHAPES.outputType_SHAPES
transmissionMode_SHAPES $OutputSection_SHAPES.transmissionMode_SHAPES]]></command>
  <inputs>
    <param name="POINTS" type="data" optional="false" label="Points" help="Points The following data types are allowed in the txt file:  x-ogc-lasf" format="txt"/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_SHAPES" title="Select the appropriate transmission mode for the output format" expanded="true">
      <param name="outputType_SHAPES" type="select" label="Author did not provide help for this parameter... ">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_SHAPES" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_SHAPES" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_SHAPES" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_SHAPES" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" ftype="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Point Cloud to Shapes]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
