<tool name="SAGA.shapes_points.21" id="saga_shapes_points_21" version="1.0.0">
  <description>Create Random Points</description>
  <requirements>
    <requirement version="3.9" type="package">python</requirement>
  </requirements>
  <version_command><![CDATA[interpreter filename.exe --version]]></version_command>
  <command><![CDATA[$__tool_directory__/Code/openapi.py isArraySHAPES False  isArrayPOLYGONS False  output_data_POINTS $output_data_POINTS  name SAGA.shapes_points.21 EXTENT $EXTENT
SHAPES $SHAPES
POLYGONS $POLYGONS
XMIN $XMIN
XMAX $XMAX
YMIN $YMIN
YMAX $YMAX
NX $NX
NY $NY
BUFFER $BUFFER
COUNT $COUNT
DISTRIBUTE $DISTRIBUTE
ITERATIONS $ITERATIONS
DISTANCE $DISTANCE
prefer $prefer
response $response
outputType_POINTS $OutputSection_POINTS.outputType_POINTS
transmissionMode_POINTS $OutputSection_POINTS.transmissionMode_POINTS]]></command>
  <inputs>
    <param name="EXTENT" type="select" optional="true" label="Target Area" help="Target Area">
      <option value="grid_system">grid system</option>
      <option value="polygons">polygons</option>
      <option value="shapes_extent">shapes extent</option>
      <option selected="true" value="user_defined">user defined</option>
    </param>
    <param name="SHAPES" type="data" optional="false" label="Shapes Extent" help="Shapes Extent The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="POLYGONS" type="data" optional="false" label="Polygons" help="Polygons The following data types are allowed in the txt file:  xml, json, vnd.google-earth.kml+xml" format="txt"/>
    <param name="XMIN" type="float" value="0" optional="true" label="Left" help="Left"/>
    <param name="XMAX" type="float" value="0" optional="true" label="Right" help="Right"/>
    <param name="YMIN" type="float" value="0" optional="true" label="Bottom" help="Bottom"/>
    <param name="YMAX" type="float" value="0" optional="true" label="Top" help="Top"/>
    <param name="NX" type="integer" value="1" optional="true" label="Columns" help="Columns"/>
    <param name="NY" type="integer" value="1" optional="true" label="Rows" help="Rows"/>
    <param name="BUFFER" type="float" value="0" optional="true" label="Buffer" help="add buffer (map units) to extent"/>
    <param name="COUNT" type="integer" value="100" optional="true" label="Number of Points" help="Number of Points"/>
    <param name="DISTRIBUTE" type="select" optional="true" label="Number for..." help="Number for...">
      <option selected="true" value="all_polygons">all polygons</option>
      <option value="each_polygon">each polygon</option>
    </param>
    <param name="ITERATIONS" type="integer" value="1000" optional="true" label="Iterations" help="Maximum number of iterations to find a suitable place for a point."/>
    <param name="DISTANCE" type="float" value="0" optional="true" label="Distance" help="Minimum distance a point should keep to each other."/>
    <param name="prefer" type="select" label="Choose the Prefer">
      <option selected="true" value="respond-async;return=representation">respond-async;return=representation</option>
      <option value="return=minimal">return=minimal</option>
      <option value="return=representation">return=representation</option>
    </param>
    <param name="response" type="select" label="Response Type" help="Choose 'raw' for raw data or 'document' for document data.">
      <option selected="true" value="document">document</option>
      <option value="raw">raw</option>
    </param>
    <section name="OutputSection_POINTS" title="Select the appropriate transmission mode for the output format and the output value" expanded="true">
      <param name="outputType_POINTS" type="select" label="Points" help="Points">
        <option value="application/json">json</option>
        <option value="application/vnd.google-earth.kml+xml">vnd.google-earth.kml+xml</option>
        <option value="text/xml">xml</option>
      </param>
      <param name="transmissionMode_POINTS" type="select" label="Choose the transmission mode">
        <option selected="true" value="reference">reference</option>
        <option value="value">value</option>
      </param>
    </section>
  </inputs>
  <outputs>
    <data name="output_data_POINTS" format="xml" hidden="false">
      <change_format>
        <when input="response" format="txt" value="document"/>
        <when input="outputType_POINTS" format="vnd.google-earth.kml+xml" value="application/vnd.google-earth.kml+xml"/>
        <when input="outputType_POINTS" format="json" value="application/json"/>
      </change_format>
    </data>
  </outputs>
  <tests>
    <test>
      <output name="output_data" value="txt"/>
      <param name="response" value="document"/>
    </test>
  </tests>
  <help><![CDATA[Create a layer with randomly distributed points.]]></help>
  <citations>
    <citation type="bibtex">Josh</citation>
  </citations>
</tool>
